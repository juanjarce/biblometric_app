@inproceedings{10.1145/3665348.3665392,
author = {Sun, Zhipeng and Zeng, Guosun},
title = {A Two-stage Coarsening Method for a Streaming Graph with Preserving Key Features},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665392},
doi = {10.1145/3665348.3665392},
abstract = {With the rapid development of 5G and mobile internet, various application fields continue to generate diverse dynamic streaming graphs with large scales and complex structures. Analyzing, querying and mining such a streaming graph face great challenges. To address the challenge of low query efficiency and avoid data loss, this paper proposes a two-stage coarsening method that preserves key features of a streaming graph while improves query efficiency. In the initial stage, the size of the streaming graph is not that large yet. To preserve its structure and attributes as much as possible, a linear mapping method is used for graph coarsening. As the size of the streaming graph increases, a graph neural network model based on cluster pooling is constructed in the second stage. Using a large number of graph coarsening results obtained in the initial stage to train and optimize this model. Then, we use this optimized model to obtain the subsequent summaries, which achieves online efficient graph coarsening. Finally, extensive experiments indicate that our algorithm significantly improves the quality of streaming graph coarsening compared to traditional algorithms. Even compared to the state-of-the-art algorithm Auxo, our algorithm shows a better performance on preserving key features.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {253–260},
numpages = {8},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inbook{10.1145/3728725.3728729,
author = {Wang, Yawen and Jin, Yuanyuan},
title = {Knowledge-Enhanced Event Prediction of Carrier-Based Aircraft Emergencies},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728729},
abstract = {Shipboard emergencies can easily induce secondary events because of any time and space transition. Carrier-based aircraft emergencies have complexity, tight coupling and strong real-time characteristics. Therefore, it is of great significance to reasonably predict the development trend of the incident in the emergency treatment to reduce the harm. Due to the sudden and random nature of naval battlefield accidents, some incidents are not covered by the historical case base. Traditional retrieval approaches to event prediction using case knowledge are limited by the sparsity of events and the cold-start problem. To address these challenges, this paper proposes a multi-graph neural network for event development trend prediction, which disassembles events into event features and introduces the shipboard aircraft domain knowledge graph. We produce a data set around the event features of carrier-based aircraft on the simulation platform. The results show that the graph neural network with knowledge graph is superior in modeling the higher-order semantic association between event features. Compared to state-of-the-art baseline methods, our approach exhibits significant improvements.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {20–25},
numpages = {6}
}

@inbook{10.1145/3728725.3728806,
author = {Han, Ze and Su, Yu and Mo, Qi and Nie, Xinyu},
title = {Neural Network Student Cognitive Diagnosis Model Based on Multi-Relational Graph},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728806},
abstract = {The Internet has enabled online learning to become an important means for students to acquire knowledge, and student cognitive diagnosis models have played a key role in this process. These models can evaluate students' learning behaviors and knowledge mastery, providing support for personalized learning. Unfortunately, traditional models have their limitations when dealing with complex relational data related mainly to the relationship between students and problems, while the characteristics as well as potential relationships among students are almost left neglected. In response, this paper proposes a Neural Network Student Cognitive Diagnosis Model Based on Multi-Relational Graph (MGNCD) that brings together the powers of Graph Attention Network (GAT) and Relational Graph Convolutional Network (RGCN) to model multi-dimensional heterogeneous nodes such as students, classes, problems, and skills. The introduction of various edge types and random edge connection strategies allowed the model to tackle data sparsity and heterogeneity. Experimental results indicate that the proposed model outperforms traditional methods in accuracy (ACC) and area under the curve (AUC), which provides an encouraging novel insight into cognitive diagnosis.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {515–521},
numpages = {7}
}

@inproceedings{10.1145/3665348.3665381,
author = {Yuan, Mengzhen and Lu, Lili},
title = {Multimodal transportation path optimization of consumer electronics products considering transportation time uncertainty},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665381},
doi = {10.1145/3665348.3665381},
abstract = {This paper focuses on the multimodal transportation path optimization problem for consumer electronics products under the condition of transport time uncertainty. Approaching the problem from the perspective of multimodal transport operators within a defined network, the study integrates the stochastic travel time, stochastic transfer time, customer demands for timely delivery and the complexity of multiple orders. Combining with the high-value time-sensitive characteristics of consumer electronic products and taking the minimum of total cost as the optimization objective, we establish a multimodal transport path optimization model. In order to solve this model, a genetic algorithm with elite strategy based on Monte Carlo sampling is designed. Finally, we verify the validity of the model and algorithm through a specific case. And the case results are analyzed. The case results indicate that for the long-distance transportation of goods such as consumer electronics, which have a very high time-value cost, it is more appropriate to choose a multimodal transportation scheme based on road and railroad transportation, while waterway transportation does not have an advantage. In addition, further consideration of the uncertainty of the transportation time results in a transportation solution that ensures a higher probability of on-time delivery, while the total cost increases slightly but the transportation time decreases. The transportation scheme is more stable. The results of this paper provide a reference basis for the actual multimodal transportation path optimization problem of consumer electronics.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {184–193},
numpages = {10},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3657054.3657124,
author = {Esposito, Mark and Tse, Terence},
title = {Mitigating the Risks of Generative AI in Government through Algorithmic Governance},
year = {2024},
isbn = {9798400709883},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3657054.3657124},
doi = {10.1145/3657054.3657124},
abstract = {The launch of the generative artificial intelligence (gen AI) application ChatGPT by OpenAI launched artificial intelligence into public discourse and led to a wave of mass uptake of this technology in organizations in the private sector. At the same time, AI is increasingly incorporated into government functions and the public sector. We propose that governments and the public sector can set an example for the responsible use of AI technologies by following the principles of algorithmic governance traditionally recommended to the private sector. Algorithmic governance has traditionally been defined in the literature as governance by algorithms, or how artificial intelligence is used to make governance decisions and affect social ordering. However, we take an alternative approach; instead, we conceptualize algorithmic governance as the governance of algorithms. We begin by summarizing the risks of generative AI use in government, then outline algorithmic governance principles, a step-by-step approach to implementing algorithmic governance into government or public sector projects, opportunities for inter-sector collaboration, and final conclusions.},
booktitle = {Proceedings of the 25th Annual International Conference on Digital Government Research},
pages = {605–609},
numpages = {5},
keywords = {AI ethics, Algorithmic Governance, Artificial intelligence, Generative AI, Government, Public sector},
location = {Taipei, Taiwan},
series = {dg.o '24}
}

@inproceedings{10.1145/3677045.3685502,
author = {Sheahan, Jacob and Cardamone, Elisa and Vines, John},
title = {Exploring Generative Postcard Futures with Older Adults},
year = {2024},
isbn = {9798400709654},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3677045.3685502},
doi = {10.1145/3677045.3685502},
abstract = {Speculative approaches to technology-enabled futures of ageing often struggle against reductive and immaterial imaginaries that diverge from everyday realities. This is compounded by the mainstream embrace of generative artificial intelligence (AI) tools that reinforce many of the stigmas and biases we have long sought to reduce in technology studies. In seeking innovative ways to critique AI-generated futures and realize novel ways of speculating on the desirable, everyday futures older adults seek, we detail emergent findings from a series of workshops employing a generative postcard approach with a local community group in Scotland. Drawing on a previous analysis of opportunities to support care in later life through an ecological carescape framework, workshops captured precarious visions, generational perspectives, and potential ways of reconfiguring care futures. We highlight how the techno-deterministic visions of generative AI are both a feature and tool for critique as we link ageing futures with everyday realities of the present.},
booktitle = {Adjunct Proceedings of the 2024 Nordic Conference on Human-Computer Interaction},
articleno = {32},
numpages = {6},
keywords = {design fiction, futures, generative AI, older people},
location = {Uppsala, Sweden},
series = {NordiCHI '24 Adjunct}
}

@inproceedings{10.1145/3734921.3734935,
author = {Chen, Lulu and Xu, Feifei},
title = {Based on Consistency Prototype Learning and Multi-knowledge Distillation for Few-Shot Continual Relation Extraction},
year = {2025},
isbn = {9798400713460},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3734921.3734935},
doi = {10.1145/3734921.3734935},
abstract = {Few-Shot Continuous Relation Extraction (FSCRE) has been plagued by issues such as overfitting, catastrophic forgetting, and feature distortion. This paper proposes a few-shot incremental relation extraction model based on consistency prototype learning and multi-layer knowledge distillation. The model consists of data augmentation, consistency prototype learning, and dual-layer knowledge distillation. First, data augmentation is applied to the training data to alleviate common overfitting problems in few-shot scenarios. Second, consistency prototype learning is utilized to regularize the embedding space and address feature distortion. Additionally, a dual-teacher model for knowledge distillation is designed to prevent catastrophic forgetting of previously acquired knowledge. Experiments conducted on the TACRED dataset show that after completing all tasks under the 5-way-5-shot setting, accuracy improved by 2.22%. Our model significantly outperforms other baseline methods, validating the effectiveness of our proposed approach.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence and Digital Media},
pages = {84–88},
numpages = {5},
keywords = {Incremental learning, Incremental relation extraction, Knowledge distillation},
location = {
},
series = {GADM '25}
}

@inproceedings{10.1145/3734921.3734936,
author = {Xu, Feifei and Chen, Lulu},
title = {A Lightweight Multi-level feature Mixing network For Image Super-Resolution},
year = {2025},
isbn = {9798400713460},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3734921.3734936},
doi = {10.1145/3734921.3734936},
abstract = {With the development of digital image technology, single image super-resolution (SISR) has made unprecedented progress. However, the existing SISR technology based on convolutional neural network (CNN) still faces great challenges in the application of resource-constrained mobile devices. For lightweight SISR, we propose a multi-level feature mixing network (MFMN), which achieves high-quality super-resolution image reconstruction at low computational cost. Based on re-parameterization, a multi-level feature generation module (MFGM) is designed to extract feature mappings from different perspectives. Then, to enhance feature details, a feature refinement module (FRM) is constructed to aggregate high-frequency information, which can assign weight values to pixels in feature maps according to the importance of different hierarchical features. Through many experiments, our proposed method shows excellent image reconstruction results in a lightweight way on multiple benchmark datasets and exceeds the current advanced methods.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence and Digital Media},
pages = {89–94},
numpages = {6},
keywords = {convolutional neural network (CNN), feature refinement, multi-level feature, single image super-resolution (SISR)},
location = {
},
series = {GADM '25}
}

@inbook{10.1145/3736252.3742615,
author = {He, Kevin and Shorrer, Ran and Xia, Mengjia},
title = {Human Misperception of Generative-AI Alignment: A Laboratory Experiment},
year = {2025},
isbn = {9798400719431},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3736252.3742615},
abstract = {We conduct an incentivized laboratory experiment to study people's perception of generative artificial intelligence (GenAI) alignment in the context of economic decision-making. Using a panel of economic problems spanning the domains of risk, time preference, social preference, and strategic interactions, we ask human subjects to make choices for themselves and to predict the choices made by GenAI on behalf of a human user. These problems confront agents with trade-offs (e.g., higher payoff vs. earlier payoff, efficiency vs. equity, riskier but potentially higher rewards vs. safer but lower rewards) and the optimal choices depend on the agent's preferences. We find that people overestimate the degree to which GenAI choices are aligned with human preferences in general (anthropomorphic projection), and with their personal references in particular (self projection). On average, human subjects' predictions about GenAI's choices in every decision environment are much closer to the average human-subject choice than to the average GenAI choice. At the individual level, human subjects' predictions about GenAI's choices in a given environment are highly correlated with their own choices in the same environment.We explore the implications of anthropomorphic projection and self projection in a stylized theoretical model. Our theoretical analysis shows that anthropomorphic projection and self projection can lead to over-delegation to GenAI. More subtly, we also find that objectively improving AI alignment can harm agents who exhibit anthropomorphic projection (because they mistakenly adjust their delegation decisions in a detrimental fashion). Similarly, among agents who exhibit self projection, welfare may be higher for those who have more unusual preferences (since they are less likely to mistakenly delegate).The full paper can be found at: https://arxiv.org/abs/2502.14708},
booktitle = {Proceedings of the 26th ACM Conference on Economics and Computation},
pages = {739},
numpages = {1}
}

@inproceedings{10.1145/3734921.3734934,
author = {Wang, Shuo and Lu, Yuqi and Luo, Ruijue and Li, Boxuan and Zhang, Yingjie and Xiao, Liang},
title = {A BERT-based sentiment analysis model for depressive text},
year = {2025},
isbn = {9798400713460},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3734921.3734934},
doi = {10.1145/3734921.3734934},
abstract = {Depression is a big public health problem around the world. Like the traditional methods of screening, clinical assessments, and self-rating scales are among the limits. These methods are based on the subjectivity of patient reports and the experience of doctors and clearly can have bias. In addition, they do not permit real-time monitoring of changes in mood. For this reason, this field of mental health is challenged with accurately and efficiently detecting probable mood changes in individuals at risk for depression. To resolve these challenges, we then propose a BERT-based sentimental analysis model adapted for depression-related text. Current methods for screening for traditional depression and established sentiment analysis models are hindered by data scarcity, data imbalance, and the problems of deriving emotional changes in a dynamic and clean manner. To leverage the strengths of BERTs in our model, we train the last layer of BERTs and then construct a classification layer tailored to the depressive emotion, which is more sensitive to the semantic information of the mood. Moreover, we also present data augmentation in its simplest form, which is the rearrangement of positive samples to increase the diversity of samples while still preserving the semantic meaning. To tackle data imbalance, we introduce a dynamic weight mechanism to the BCEWithLogitsLoss function by adjusting the weight of the positive samples with data distribution. The model effectively captures the complex emotional changes by combining global and local semantic dynamic representations. In our empirical study, we analyze the open-source dataset of interview transcripts of 162 volunteers. Our model is evaluated using the experimental results, and the precision, the recall rate, and the F1 score obtained are 0.667, 0.667, and 0.667, respectively. Our model has achieved 2.62 higher in terms of precision and F1 score as compared to the existing models. The results indicate that our model provides a big benefit in identifying screened individuals at risk for depression.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence and Digital Media},
pages = {76–83},
numpages = {8},
keywords = {BERT model, Depression screening, medical text processing, sentiment analysis, text classification},
location = {
},
series = {GADM '25}
}

@inproceedings{10.1145/3573382.3616030,
author = {Phillips, Cody and Trick, Nico and Nacke, Lennart and Mandryk, Regan},
title = {The Role of Generative AI in Games Research},
year = {2023},
isbn = {9798400700293},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3573382.3616030},
doi = {10.1145/3573382.3616030},
abstract = {Generative artificial intelligence (generative AI) has seen a meteoric rise in capability, popularity, and use. As the popularity of this technology grows, protocols are being developed around how to manage the use of generative AI in academic spaces. It is imperative that the games and play research community join this important conversation, and discuss the appropriateness of using generative AI in both digital games, and our own academic works.},
booktitle = {Companion Proceedings of the Annual Symposium on Computer-Human Interaction in Play},
pages = {353–354},
numpages = {2},
keywords = {GPT, Generative AI, digital games, panel},
location = {Stratford, ON, Canada},
