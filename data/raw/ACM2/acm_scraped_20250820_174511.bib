@inproceedings{10.1145/3706598.3713093,
author = {Duan, Wen and Li, Lingyuan and Freeman, Guo and McNeese, Nathan},
title = {A Scoping Review of Gender Stereotypes in Artificial Intelligence},
year = {2025},
isbn = {9798400713941},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3706598.3713093},
doi = {10.1145/3706598.3713093},
abstract = {People often apply gender stereotypes to Artificial Intelligence (AI), and AI design frequently reinforces these stereotypes, perpetuating traditional gender ideologies in state-of-the-art technology. Despite growing interests in investigating this phenomenon, there is little conceptual clarity or consistency regarding what actually constitutes a "gender stereotype" in AI. Therefore, it is critical to provide a more comprehensive image of existing understandings and ongoing discussions of gender stereotypes of AI to guide AI design that reduces the harmful effects of these stereotypes. In doing so, this paper presents a scoping review of over 20 years of research across HCI, HRI and various social science disciplines on how gender stereotypes are applied to AI. We outline the methods and contexts of this growing body of work, develop a typology to clarify these stereotypes, highlight under-explored approaches for future research, and offer guidelines to improve rigor and consistency in this field that may inform responsible AI design in the future.},
booktitle = {Proceedings of the 2025 CHI Conference on Human Factors in Computing Systems},
articleno = {995},
numpages = {20},
keywords = {Artificial Intelligence, Gender stereotypes, Anthropomorphism},
location = {
},
series = {CHI '25}
}

@inproceedings{10.1145/3744367.3744422,
author = {Wu, Guijie and Lin, Qingqing},
title = {Artificial intelligence-driven innovation in language teaching and learning in vocational colleges: paths, practices and challenges},
year = {2025},
isbn = {9798400715068},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3744367.3744422},
doi = {10.1145/3744367.3744422},
abstract = {The “Technology and Humanities Integration” teaching model aims to seamlessly integrate advanced technologies with humanities education principles, thereby enriching the experience of Chinese Language classrooms. Intelligent assessment systems powered by powerful data processing and analysis capabilities can accurately assess teaching efficiency and student learning progress. The collaborative framework between teachers and technology emphasizes the optimal use of teachers’ professional skills, supported by advanced technologies, thereby improving teaching efficiency and quality. Regarding the application of technology, semantic analysis is performed using the BERT model, the random forest algorithm is used to optimize teaching scenarios, and the structural equation model is applied to evaluate moral hazard. Chinese teaching practice using virtual reality technology shows that students' cultural perception ability has improved by 42%, a finding that has been verified by rigorous statistical tests (P &lt; 0.001) and is therefore very reliable. In addition, the writing aids used significantly improved students’ Chinese writing skills, with students using these tools increasing their average Chinese score by 15%. These empirical data firmly affirm the value of cutting-edge technology in language education in vocational schools.},
booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Educational Systems},
pages = {345–352},
numpages = {8},
keywords = {Artificial Intelligence, Ethical Governance, Multimodal Learning, Natural Language Processing, Vocational Education},
