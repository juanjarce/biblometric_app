series = {ICAIE '24}
}

@inproceedings{10.1145/3706890.3706982,
author = {Qi, Xuge and Huang, Aiwen and Li, Bei and Song, Wandeng and Xiao, Yang},
title = {Hotspots and Future Trends of Artificial Intelligence in Traditional Chinese Medicine: A Systematic Bibliometric Analysis},
year = {2025},
isbn = {9798400717826},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3706890.3706982},
doi = {10.1145/3706890.3706982},
abstract = {Background: This study will comprehensively analyze the current research status and application of artificial intelligence in traditional Chinese medicine using systematic bibliometric methods. This study also explores the emerging research hotspots and future development trends in this field, providing valuable references for related scholars. Methods: We systematically retrieved relevant publications on the research and application of artificial intelligence in traditional Chinese medicine from 2003 to 2022 using the Web of Science. Graph making, statistical analysis, and visualization studies were performed using Microsoft Office Excel (version 2021), CiteSpace (version 6.2.R4), and VOSviewer (version 1.6.19.0) software. Results: As of October 30, 2023, we have identified 722 publications with a citation frequency 9135. Notably, China exhibits remarkable dominance in terms of publication counts (86.15%), citation counts (6931), and h-index (41). Furthermore, Chen, Calvin Yu-Chian of Sun Yat-sen University has contributed significantly with the highest number of publications at 13. Additionally, deep learning methods have emerged as the predominant algorithms for artificial intelligence in recent years, and the diagnosis of image segmentation, primarily feature extraction-based tongue diagnosis, has garnered substantial research attention. Conclusion: The bibliometric analysis revealed that using deep learning approaches for diagnosis and prediction represents a burgeoning area of research interest. Moreover, the prospective clinical application of artificial intelligence in traditional Chinese medicine is poised to become a prominent subject shortly.},
booktitle = {Proceedings of the 2024 5th International Symposium on Artificial Intelligence for Medicine Science},
pages = {539–544},
numpages = {6},
keywords = {Artificial Intelligence, Bibliometrics, Traditional Chinese Medicine},
location = {
},
series = {ISAIMS '24}
}

@inproceedings{10.1145/3724979.3725056,
author = {Zhou, Xuan and Sui, Huajie and Guo, Yuhong and Zou, Ziyi},
