@inproceedings{10.1145/3665348.3665398,
author = {Zhao, Wei and You, Junling and Chen, Qinghui},
title = {SQL Injection Attack Detection Based on Text-CNN},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665398},
doi = {10.1145/3665348.3665398},
abstract = {In the digital age of the 21st century, with the widespread use of Web applications, SQL injection attacks have become one of the significant challenges in the field of information security. This study proposes a method for detecting SQL injection attacks based on an improved Text-CNN model. The method starts with the preprocessing of collected SQL injection attack data, followed by the use of the convolutional layers of the Text-CNN model to extract local features, and then combines the CAM attention mechanism to achieve the task of SQL injection detection. The introduction of the CAM attention mechanism aims to merge the information from max pooling and average pooling to reduce information loss and enhance the global representation capability of the model. Experimental results show that the proposed method has improved in terms of accuracy, precision, recall rate, and F1 score, with values of 92.40%, 92.45%, 92.29%, and 0.9237, respectively. Given that traditional Text-CNN, Text-RNN, and CNN algorithms can perform well in recognizing SQL injection attacks, this provides help for subsequent applications in network security protection.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {292–296},
numpages = {5},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665399,
author = {Zhang, Mingjie and Zhu, Longzhu and Ding, Maomao and Liu, Lu and Gong, Lihua and Gong, Jian},
title = {95598 Customer Service Complaint Spillover Risk Prediction Method Based on Data Association Clustering},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665399},
doi = {10.1145/3665348.3665399},
abstract = {Conventional 95598 customer service spillover risk prediction method mainly uses SMOTE (Synthetic Minority Oversampling) minority oversampling method to deal with the prediction samples, which is susceptible to the influence of weighted fusion, resulting in poor prediction results, therefore, a new 95598 customer service spillover risk prediction method needs to be designed based on the correlation clustering of data, i.e., collecting and processing the complaint spillover risk prediction data, designing the customer service spillover risk prediction algorithm using data correlation clustering, and thus realizing the customer service spillover risk prediction. Therefore, it is necessary to design a new 95598 customer service complaint spillover risk prediction method based on data association clustering, i.e., we collected and processed the complaint spillover risk prediction data, and designed a customer service complaint spillover risk prediction algorithm by using data association clustering, so as to realize the customer service complaint spillover risk prediction. The experimental results show that the designed customer service complaint spillover risk prediction method based on data association clustering has good prediction effect, and the predicted root mean square error (RMSE), predicted mean absolute percentage error (MAPE), and predicted mean absolute error (MAE) are low, reliable, and have certain application value, which makes a certain contribution to the improvement of customer service quality.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {297–302},
numpages = {6},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665362,
author = {Cao, Limin and Hu, Yuxia and Nian, Fudong},
title = {Semi-supervised Crowd Counting in Dense Scenes via Self-training},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665362},
doi = {10.1145/3665348.3665362},
abstract = {Crowd counting in dense scenes is a crucial task in the field of computer vision that aims to estimate crowd statistics by predicting the location of heads. However, the accuracy of detection algorithms usually depends on manual precision labeling of a vast amount of data, which is less feasible in practical scene applications due to its high labeling cost. Moreover, the manual labeling process is prone to errors such as false labeling and missing labeling due to overlapping and small targets in the image. To address these challenges, this paper proposes a pseudo label generation algorithm based on semi-supervised learning for dense crowd datasets. The proposed algorithm employs the self-training method of target detection in semi-supervised learning to generate pseudo-labels offline. Furthermore, the algorithm uses existing point labels in dense crowd datasets to correct the positioning information, which significantly reduces the high labeling cost. The proposed algorithm is evaluated on three widely used benchmarks, ShanghaiTech, NWPU-Crowd, and UCF-QNRF, and the experimental results demonstrate the effectiveness of the method.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {72–76},
numpages = {5},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665380,
author = {Yan, Liuqing and Yu, Shihu},
title = {Study on English expression polishing system based on deep learning},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665380},
doi = {10.1145/3665348.3665380},
abstract = {In general, grammar polishing systems recombine words and their order of combination based on the comparison results of word combinations and large vocabulary grammar data to achieve error polishing. However, this process is limited by the storage capacity of the large vocabulary grammar, which hampers the effectiveness and efficiency of error polishing. To overcome this constraint, deep learning algorithms are employed. These algorithms involve reconstructing the underlying hardware framework and optimizing the error polishing calculation parameters of the system software using deep learning algorithms. This allows for global replacement of deep learning algorithms, enhances the system's self-learning ability for error polishing, and frees it from the limitations of large vocabulary grammar information. The test results demonstrate that the continuous error polishing ability of this approach is superior. Among the error polishing systems tested, the designed system exhibits the best overall stress test performance, showcasing strong stability and control. The overall stability demonstrated by the stress test aligns with the standards and requirements of practical applications. As a result, the design system possesses good overall error polishing ability, system stability, and practical applicability.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {179–183},
numpages = {5},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665354,
author = {Niu, Lianqiang and Bao, Han},
title = {Fast Tactile Paving Segmentation Model Based on Reparameterized Structure},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665354},
doi = {10.1145/3665348.3665354},
abstract = {In order to solve the problem of low segmentation accuracy due to the interference of noise information such as light and shadow in the current lightweight blind segmentation model, we propose a fast tactile paving segmentation model based on reparameterize. The multi-branch training structure learns spatial features such as tactile paving, light and shadow while compensating for the loss of information in the feature map due to downsampling operations as well as enhancing the learning of its features in the channel dimension. The single-branch structure obtained after reparameterization of the multibranch structure ensures faster inference of the model as well as lower memory access costs. The introduction of coordinate attention allows the model to learn the distribution law of the features of tactile paving, light and shadow, etc. in the spatial dimension and automatically generate the attention map with weight information, effectively reducing the weight of the spatial structural features of noise such as light and shadow. Experiments show that the model's ability to resist interference from environmental noise such as light and shadow has been enhanced and the segmentation accuracy has been further improved under the premise of ensuring the inference speed.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {24–28},
numpages = {5},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665353,
author = {Wang, Shen and Cai, Fei and Zhang, Mengxi},
title = {Multilingual Event Causality Identification via Meta-learning with Knowledge},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665353},
doi = {10.1145/3665348.3665353},
abstract = {Multilingual Event Causality Identification (Multilingual ECI) is the task of detecting causal relations between events mentioned in the multilingual text. The biggest challenge of this task is the small size of the minor language corpus, making it very difficult to train a model with good performance. To address this issue, we propose a meta-learning method that integrates multilingual knowledge, which mainly includes two modules: the multilingual knowledge obtain module, which alleviates data scarcity by introducing multilingual knowledge from outside; The meta-learning module utilizes ProtoMAML combined with l₂ standardization to extract language independent causal features from text and learn better event representations in different languages. The experimental results on the latest publicly available MECI dataset show that our model outperforms the current best baseline, with an average F1 improvement of 3.9% in different languages.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {18–23},
numpages = {6},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665405,
author = {Liang, Xin and Gao, Maoting},
title = {The road crack detection algorithm improved based on YOLOv7},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665405},
doi = {10.1145/3665348.3665405},
abstract = {We propose an algorithm for road crack detection, leveraging enhancements to YOLOv7. This approach aims to overcome challenges related to the limited capacity for extracting features from irregularly shaped road crack targets and the sluggish inference speed caused by the intricate model structure. In this algorithm, YOLOv7 is used as the foundation. Firstly, the Detect module is improved to a Decoupled Head Detect module to augment the detection capacity of the model on feature maps. Then, the Head part is augmented with an ELAN-G module based on GSConv to reduce the model's parameter computation and accelerate the inference speed. Finally, Deformable Conv is employed to modify the original ELAN module into an ELAN-D module and integrate it into the Backbone part to bolster the model's capability in extracting irregular-shaped crack targets. Experimental results on the CRDDC-2020-US dataset demonstrate that the improved algorithm achieves a 10% enhancement in detection accuracy, with the mAP(0.5) increasing from 53.7% to 59.4%. Furthermore, the model's parameter computation is decreased by 12%, while the throughput in terms of the number of images processed per second during inference is boosted by 13%. This algorithm proves to be effective for road crack detection applications.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {335–339},
numpages = {5},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665384,
author = {Liu, Wenzhao and Yang, Kuiwu and Chen, Yue and Dai, Huanyao},
title = {Exploring Robustness under New Adversarial Threats: A Comprehensive Analysis of Deep Neural Network Defenses},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665384},
doi = {10.1145/3665348.3665384},
abstract = {Although various methods have been proposed to improve the robustness of deep neural network models, evaluating these models in a fair and reasonable manner remains a challenge. Existing evaluation methods often consider only limited types of attacks, ignoring the generalization performance against new types of attacks, and fail to cover the most advanced defense models. To address these issues, we propose a unified framework to evaluate model robustness under different types of attacks. This framework integrates non-Lp and Lp attacks, and comprehensively evaluates the robustness of various advanced robust models on the CIFAR-10 and ImageNet subsets. Experimental results show that even the most advanced defense models exhibit vulnerabilities under certain new types of attacks, highlighting the importance of developing more comprehensive robustness benchmarks and providing guidance for the design of future robust deep learning models.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {206–210},
numpages = {5},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665404,
author = {Du, Peng and Song, Xudong},
title = {Lightweight Target Detection: An Improved YOLOv8 for Small Target Defect Detection on Printed Circuit Boards},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665404},
doi = {10.1145/3665348.3665404},
abstract = {Addressing the performance and efficiency challenges of detecting small target defects on PCB (printed circuit board), this research proposes a lightweight inspection model for YOLOv8-based. The initial step involves employing an enhanced Efficient_Detect network architecture with the aim of diminishing the volume of model parameters and computational intricacy, while simultaneously augmenting resource utilization. Then, the deformable convolutional networks v2 (DCNv2) are integrated into the C2f structure to improve feature extraction for defects with diverse shapes on the PCB surface, enhancing the model's capability to identify different categories of flaws. Meanwhile, the Inner-SIoU loss function is utilized instead of the conventional CIoU loss function to enhance both the precision of inspection and the robustness of the model. The enhanced approach achieves an mAP50 of 96.8% and an mAP50-95 of 72.2%, marking an improvement of 3.9% and 10.1%, respectively, comparison with base YOLOv8n model. In addition, the parameter counts of the model, GFLOPs, and weight size are notably reduced by 18.4%, 33.3%, and 19.7%, respectively, resulting in final metrics of 2.34 M, 5.4 GFLOPs, and a weight size of 4.9 M. In addition, the detection speed can reach up to 161 FPS, which has the potential for real-time detection. It provides an efficient and lightweight PCB small target defect detection solution for scenarios with limited computing resources.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {329–334},
numpages = {6},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665361,
author = {Du, Xiao and Zhou, Jing and Gao, Xu},
title = {Numerical Simulation of Receiving Transducer for Impulse Sound Source Depth Detection},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665361},
doi = {10.1145/3665348.3665361},
abstract = {Receiving transducer is one of the core components of impulse sound source depth detection instrument, and its performance directly affects the accuracy of measurement signal. In this paper, the influence of the structure size, circuit connection mode and fixing mode of the 0kHz-35kHz receiving transducer on the performance of the receiving transducer is numerically simulated. It is concluded that shorter and thicker piezoelectric patches can effectively improve the receiving sensitivity, bandwidth and stability of the receiving transducer. The series circuit connection mode has good reception sensitivity stationarity, and the reception sensitivity stationarity is improved by 23.8dB, but its reception sensitivity stationarity is extremely poor. The fixed boundary condition at both ends can obviously broaden the receiving bandwidth of impulse sound source receiver and transducer, but the receiving sensitivity of fixed boundary condition at both ends is smaller than that of free boundary condition and free boundary condition at one end. This conclusion has theoretical reference significance for the manufacture of impulse sound source receiving transducer.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {67–71},
numpages = {5},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665369,
author = {Jiang, Lianyuan and Chen, Jinlong},
title = {A Meta Learning Few Shot Object Detection Algorithm Based on PRN-FPN},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665369},
doi = {10.1145/3665348.3665369},
abstract = {The goal of Few Shot Object Detection (FSOD) task is to effectively train and deploy object detection models even in small datasets, in order to achieve high-precision object detection results. Due to the few shot size of the dataset, the model may have difficulty learning sufficiently generalized feature representations, which can lead to overfitting or underfitting issues. Specifically, the model performs well during training, but shows a significant poor performance on test. That's because model overfits the noise and specific samples of the data in the training set, resulting in the inability to generalize to new data. Secondly, in the case of few shots, the target category scale in the dataset is single, making it difficult for the model to distinguish the same target at different scales, which affects detection accuracy. In order to effectively perform FSOD tasks, this paper proposes a meta learning FSOD algorithm based on PRN-FPN. Firstly, a data augmentation module is proposed to address the issue of data scarcity in few shot situations. The module includes three conversion strategies: color, color, and histogram equalization. Combining these three strategies can effectively increase the diversity of scarce data. In order to improve the algorithm's recognition and detection of targets of the same category but different scales, a Predictor head Removing Network with Feature Pyramid Network (PRN FPN) module is proposed. The module adds CARAFE Feature Pyramid Network on the basis of Predictor head Removal Network (RPN), and has the ability to detect few shot objects at multiple scales by fusing features at different depths. Finally, the algorithm was trained and tested on the publicly available dataset Pascal VOC, and the experimental results showed its effectiveness.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {113–119},
numpages = {7},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665402,
author = {Chen, Kang and Chen, Shanshan and Cao, Bing and Wang, Menglong and Pei, Lang},
title = {Research on PCB Flying Needle Test Technology Based on Improved Ant Colony Algorithm},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665402},
doi = {10.1145/3665348.3665402},
abstract = {The integration of printed circuit boards (PCB) is getting higher and higher, which undoubtedly has higher and higher testing requirements for printed circuit boards. Flying needle test is the main means of PCB test at present, the quality of the test path directly affects the test efficiency. In this paper, a technique of PCB flying pin test based on improved ant colony algorithm is studied, and its flow and rules are introduced. Through experimental verification, it can be proved that the proposed algorithm can effectively shorten the path length and improve the test efficiency.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {319–323},
numpages = {5},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3665348.3665393,
author = {Tang, Jiale},
title = {Dual-Decoder Importance-Aware Gated Network for RGB-T Salient Objection Detection},
year = {2024},
isbn = {9798400709562},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3665348.3665393},
doi = {10.1145/3665348.3665393},
abstract = {Most recent algorithms of RGB-T salient object detection have two issues remain unresolved. The first one is that existing methods don't fully explore and exploit the potentials of complementary of different modalities and multi-type cues of images, which play a vital role in achieving accurate results. And the second is that it is difficult to determine which of the two modalities contributes more in the feature fusion stage.To address these problems, we propose a dual-decoder importance-aware gated network called LNet. Specifically, we set the dual-decoder architecture to solve the cross-modality interaction problem. It can dig out the characteristics of two modalities, and fuse the cross-modality complementarity information more effectively. In particular, the importance-aware gated part Screening Mechanism (SM) can suppress saliency bias and let model avoid obtaining the results dominated by one modality.Experimental results demonstrate that the proposed LNet achieves better performance than the 11 state-of-the-art on three public datasets.},
booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
pages = {261–265},
numpages = {5},
location = {Kuala Lumpur, Malaysia},
series = {GAIIS '24}
}

@inproceedings{10.1145/3734921.3734927,
author = {Wang, Zhenyu and Zhang, Xiangchun and Zhang, Xiaoqing and Zeng, Zhaoyang and Peng, Wensheng and Yan, Jia},
title = {Study of Bi-loop BERT-based Corner Tagging Joint Event Extraction},
year = {2025},
isbn = {9798400713460},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3734921.3734927},
doi = {10.1145/3734921.3734927},
abstract = {A Bi-loop BERT-based Corner Tagging (Bi-BCT) joint event extraction model is introduced to enhance aviation accident report analysis by addressing cascading errors and redundancy. Leveraging pre-training models, triggers are identified. Context and features are merged to compute argument confidence. Corner tagging matrix is used for triples decoding. Textual specifics of aviation accidents are analyzed to expand the Aviation Accident Report (AAR) dataset, boosting its quality by 7.05%. Experimental results on the AAR dataset reveal Bi-BCT outperforms the Onerel model with a 9.2% accuracy, 7.2% recall, and 8.2% F1 value improvement, showcasing its efficacy in aviation accident information extraction.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence and Digital Media},
pages = {32–40},
numpages = {9},
keywords = {Aviation accidents, Datasets, Deep learning, Event extraction, Large Language models},
