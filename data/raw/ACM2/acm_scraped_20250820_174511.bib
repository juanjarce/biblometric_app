@inproceedings{10.1145/3732801.3732826,
author = {Gu, Yadi},
title = {The Impact of Chatbot-assisted Learning Model on Higher-Order Thinking Skills (HOTS) in a 3D Animation Design Course},
year = {2025},
isbn = {9798400712432},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
