}

@inproceedings{10.1145/3734921.3734929,
author = {Zhou, Wanzhen and Guo, Haohao and Wang, Jianxia},
title = {Development of a Child-Adult Binding Access Control System Using Face Recognition},
year = {2025},
isbn = {9798400713460},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3734921.3734929},
doi = {10.1145/3734921.3734929},
abstract = {Aiming to address the need for child supervision in closed public spaces, this paper proposes an intelligent access control system based on face recognition. The system adopts lightweight MTCNN and MobileFaceNet algorithms to dynamically bind the identities of children and adults by generating unique ID codes (timestamp + random six-digit number). During entry, an adult generates an ID code via face detection and associates it with the child's information. During exit, a child can only pass if their ID code matches the associated adult's ID. Experimental results demonstrate that the system achieves a recognition accuracy of 99.93%, a single binding time of 2.8 seconds, and a database response time of less than 10 ms under high concurrency (1000 QPS). This system enables accurate guardianship without pre-registration and effectively reduces the risk of children getting lost or being taken by unauthorized individuals.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence and Digital Media},
pages = {47–51},
numpages = {5},
keywords = {Child protection, Dynamic identity binding, Face recognition, No pre-registration},
location = {
},
series = {GADM '25}
}

@inproceedings{10.1145/3713043.3728836,
author = {Dangol, Aayushi and Zhao, Runhua and Wolfe, Robert and Ramanan, Trushaa and Kientz, Julie A. and Yip, Jason},
title = {“AI just keeps guessing”: Using ARC Puzzles to Help Children Identify Reasoning Errors in Generative AI},
year = {2025},
isbn = {9798400714733},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3713043.3728836},
doi = {10.1145/3713043.3728836},
abstract = {The integration of generative Artificial Intelligence (genAI) into everyday life raises questions about the competencies required to critically engage with these technologies. Unlike visual errors in genAI, textual mistakes are often harder to detect and require specific domain knowledge. Furthermore, AI’s authoritative tone and structured responses can create an illusion of correctness, leading to overtrust, especially among children. To address this, we developed AI Puzzlers, an interactive system based on the Abstraction and Reasoning Corpus (ARC), to help children identify and analyze errors in genAI. Drawing on Mayer &amp; Moreno’s Cognitive Theory of Multimedia Learning, AI Puzzlers uses visual and verbal elements to reduce cognitive overload and support error detection. Based on two participatory design sessions with 21 children (ages 6 - 11), our findings provide both design insights and an empirical understanding of how children identify errors in genAI reasoning, develop strategies for navigating these errors, and evaluate AI outputs.},
booktitle = {Proceedings of the 24th Interaction Design and Children},
pages = {444–464},
numpages = {21},
keywords = {AI Literacy, Participatory design, Generative AI},
location = {
},
series = {IDC '25}
}

@article{10.1145/3710912,
author = {Guridi, Jose A. and Hwang, Angel Hsing-Chi and Santo, Duarte and Goula, Maria and Cheyre, Cristobal and Humphreys, Lee and Rangel, Marco},
title = {From Fake Perfects to Conversational Imperfects: Exploring Image-Generative AI as a Boundary Object for Participatory Design of Public Spaces},
year = {2025},
issue_date = {May 2025},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {9},
number = {2},
url = {https://doi.org/10.1145/3710912},
doi = {10.1145/3710912},
abstract = {Designing public spaces requires balancing the interests of diverse stakeholders within a constrained physical and institutional space. Designers usually approach these problems through participatory methods but struggle to incorporate diverse perspectives into design outputs. The growing capabilities of image-generative artificial intelligence (IGAI) could support participatory design. Prior work in leveraging IGAI's capabilities in design has focused on augmenting the experience and performance of individual creators. We study how IGAI could facilitate participatory processes when designing public spaces, a complex collaborative task. We conducted workshops and IGAI-mediated interviews in a real-world participatory process to upgrade a park in Los Angeles. We found (1) a shift from focusing on accuracy to fostering richer conversations as the desirable outcome of adopting IGAI in participatory design, (2) that IGAI promoted more space-aware conversations, and (3) that IGAI-mediated conversations are subject to the abilities of the facilitators in managing the interaction between themselves, the AI, and stakeholders. We contribute by discussing practical implications for using IGAI in participatory design, including success metrics, relevant skills, and asymmetries between designers and stakeholders. We finish by proposing a series of open research questions.},
journal = {Proc. ACM Hum.-Comput. Interact.},
month = may,
articleno = {CSCW014},
numpages = {33},
keywords = {artificial intelligence, design, generative AI, human-AI interaction}
}

@inproceedings{10.1145/3643834.3660720,
author = {Takaffoli, Macy and Li, Sijia and M\"{a}kel\"{a}, Ville},
title = {Generative AI in User Experience Design and Research: How Do UX Practitioners, Teams, and Companies Use GenAI in Industry?},
year = {2024},
isbn = {9798400705830},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3643834.3660720},
doi = {10.1145/3643834.3660720},
abstract = {User Experience (UX) practitioners, like UX designers and researchers, have begun to adopt Generative Artificial Intelligence (GenAI) tools into their work practices. However, we lack an understanding of how UX practitioners, UX teams, and companies actually utilize GenAI and what challenges they face. We conducted interviews with 24 UX practitioners from multiple companies and countries, with varying roles and seniority. Our findings include: 1) There is a significant lack of GenAI company policies, with companies informally advising caution or leaving the responsibility to individual employees; 2) UX teams lack team-wide GenAI practices. UX practitioners typically use GenAI individually, favoring writing-based tasks, but note limitations for design-focused activities, like wireframing and prototyping; 3) UX practitioners call for better training on GenAI to enhance their abilities to generate effective prompts and evaluate output quality. Based on our findings, we provide recommendations for GenAI integration in the UX sector.},
booktitle = {Proceedings of the 2024 ACM Designing Interactive Systems Conference},
pages = {1579–1593},
numpages = {15},
keywords = {Company Policies, GenAI, Generative Artificial Intelligence, Human-AI Collaboration, Industry Practices, Interaction Design, User Experience Design, User Experience Research},
location = {Copenhagen, Denmark},
series = {DIS '24}
}

@inproceedings{10.1145/3721241.3737653,
author = {Lee, Kevin and Kudlay, Vadim},
title = {GenAI Demystified},
year = {2025},
isbn = {9798400715433},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3721241.3737653},
doi = {10.1145/3721241.3737653},
abstract = {Generative artificial intelligence is taking the world by storm. It's a new technology that enables computers learn from historical data to generate new content that feels original and human-like. Although generative AI has become a part of everyday life, many people still lack a clear understanding of how it works. Without a solid understanding of generative AI, it can be difficult for individuals to fully embrace this emerging technology. This course offers a comprehensive introduction to the concepts, technologies, and real-world impact of generative AI.The course begins with a historical overview of artificial intelligence that traces its evolution from early rules-based logic to the deep learning breakthroughs that have enabled today's most powerful generative AI models. Participants will explore foundational concepts that include illustrations of neural network models to gain intuition behind how machines learn to generate text, images, and other types of content. The course delves into practical applications of generative AI across multiple modalities, including natural language, computer vision, audio, and more. Participants will also study high-level system architectures of an AI chatbot, the poster child of generative AI, to understand how these systems operate. Finally, the course will examine future development of generative AI, including the rise of agentic applications that can reason, plan, and interact autonomously.},
booktitle = {Proceedings of the Special Interest Group on Computer Graphics and Interactive Techniques Conference Courses},
articleno = {7},
numpages = {2},
keywords = {Artificial Intelligence, Generative AI, Machine Learning},
location = {
},
series = {SIGGRAPH Courses '25}
}

@inproceedings{10.1145/3708036.3708042,
author = {Su, Jiajia and He, Zhongjun},
title = {Enhancing User Experience Evaluation of Graphic Art Style Games through Collaboration with Generative AI},
year = {2025},
isbn = {9798400709999},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3708036.3708042},
doi = {10.1145/3708036.3708042},
abstract = {With the rapid development of generative artificial intelligence (AI) technology, its application in game design is becoming increasingly widespread. This paper aims to explore how generative AI can enhance user experience by augmenting graphic art style game design. Through the study of design theories, those theories that have a strong influence on graphic art style games are identified. Next, the paper proposes a generative AI-based game design method that combines prompts confirmation, the setting of generated images, and user feedback optimization, aiming to enhance the aesthetic quality and interactivity of the game. To evaluate the effectiveness of the method, this paper designs and implements an interview method and a questionnaire to collect data and find out the user experience of generative AI-generated graphic art style games. The experimental results show that generative AI not only significantly improves the efficiency of game design, but also allows for innovative graphics and element design. The findings of this paper provide new perspectives and methods for the application of AI in future game design and also lay the foundation for further exploring the potential of generative AI in other art style games.},
booktitle = {Proceedings of the 2024 5th International Conference on Computer Science and Management Technology},
pages = {31–38},
numpages = {8},
keywords = {Games, Generative Artificial Intelligence, Graphic Art Styles, User Experience},
location = {
},
series = {ICCSMT '24}
}

@inproceedings{10.1145/3734921.3734937,
author = {Liu, Caiyan and Liu, Yajun and Yao, Zhijie and He, Shan},
title = {Research on the Product Design of Smart Interactive Lighting for Empty Nest Youth: User Evaluation Mechanism and Optimization Strategies Based on the KONA-IoT Model},
year = {2025},
isbn = {9798400713460},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3734921.3734937},
doi = {10.1145/3734921.3734937},
abstract = {Objective:&nbsp;To address the loneliness of nearly 80-million Empty Nest Youth in China, this study proposes an optimized solution integrating the KONA model and IoT technology. The approach adopts a "technology-emotion dual-driven" strategy to enhance the home experience of contemporary youth living alone. Method:&nbsp;A four-layer IoT architecture (perception layer, network layer, cognitive layer, execution layer) and dynamic algorithms were constructed. Key demand factors (Emotional interaction SI, Nostalgic elements DSI) were extracted using the KANO model and factor analysis. Two product prototypes were designed: a radio/MP3-inspired desk lamp and a wall lamp. Performance was validated through the NASA-TLX scale and nearly 300 questionnaires. Results:&nbsp;System response latency was reduced, energy consumption decreased by approximately 1/3, Satisfaction with emotional interaction increased significantly. Cognitive load decreased, and emotional interaction satisfaction increased. However, nostalgic elements and durability require further improvement. Optimization strategies based on an importance-satisfaction matrix were proposed to provide home design products with both high efficiency and emotional support for Empty Nest Youth, In order to promote the deep integration of human-computer interaction.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence and Digital Media},
pages = {95–103},
numpages = {9},
keywords = {Empty Nest Youth, KONA-IoT Model, Smart Interactive Lighting},
location = {
},
series = {GADM '25}
}

@inbook{10.1145/3728725.3728738,
author = {Liu, Xiaomeng and Liu, Jia and Li, Jiayuan and Zhou, Yong and Xiang, Shengchang},
title = {Research on the Application of LLM in Institutional Compliance and Early Warning},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728738},
abstract = {Enterprise compliance management faces various challenges in today's business operations. This article proposes a new method for institutional compliance inspection and early warning based on LLM, which aims to focus on enterprise compliance and early warning monitoring as the core, using intelligent LLM technology, combined with artificial intelligence, machine learning and natural language processing technologies, to empower enterprise institutional revision, dynamic management, and execution, establish intelligent tools that adapt to the dynamic and efficient management needs of the system, realize the intelligence of institutional-related business and management, and promote the improvement of the institutional management system, promote lawful and compliant operation, and achieve high-quality development.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {80–86},
numpages = {7}
}

@inbook{10.1145/3676536.3698390,
author = {Jabbour, Jason and Janapa Reddi, Vijay},
title = {Generative AI Agents in Autonomous Machines: A Safety Perspective},
year = {2025},
isbn = {9798400710773},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3676536.3698390},
abstract = {The integration of Generative Artificial Intelligence (AI) into autonomous machines represents a major paradigm shift in how these systems operate and unlocks new solutions to problems once deemed intractable. Although generative AI agents provide unparalleled capabilities, they also have unique safety concerns. These challenges require robust safeguards, especially for autonomous machines that operate in high-stakes environments. This work investigates the evolving safety requirements when generative models are integrated as agents into physical autonomous machines, comparing these to safety considerations in less critical AI applications. We explore the challenges and opportunities to ensure the safe deployment of generative AI-driven autonomous machines. Furthermore, we provide a forward-looking perspective on the future of AI-driven autonomous systems and emphasize the importance of evaluating and communicating safety risks. As an important step towards addressing these concerns, we recommend the development and implementation of comprehensive safety scorecards for the use of generative AI technologies in autonomous machines.},
booktitle = {Proceedings of the 43rd IEEE/ACM International Conference on Computer-Aided Design},
articleno = {3},
numpages = {13}
}

@inproceedings{10.1145/3734921.3734938,
author = {Xiong, Qiwei and Liao, Junfeng and Liu, Detian and Chen, Jusheng and Su, Zichen and Hua, Haoqiang and Wang, Lin and Yao, Hongbo and Zhou, Zhiheng and Shu, Lin},
title = {Multi-Channel Tactile-Force Rendering: A Microcurrent-Based Computational Framework for High-Resolution Haptic Feedback},
year = {2025},
isbn = {9798400713460},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3734921.3734938},
doi = {10.1145/3734921.3734938},
abstract = {With the rapid development of information technology (IT), the lifestyle of modern society has transformed to a faster pace and is more human-centered. The developments in haptic technology have significantly improved the human-computer interaction. Virtual reality (VR) technology and the metaverse, which highlight personalized experience, have become hotspots of the information application industry. In VR applications, adding haptic feedback to the human-computer interaction process can make the experience more immersive. In recent years, research on virtual haptics has focused on two areas: the use of microcurrents to stimulate mechanoreceptors and the use of virtual haptics to convey information. Multi-channel tactile-force rendering permits realistic touch sensations. This is achieved by utilizing force and tactile feedback methods. It is effective for the users to feel detailed and immersive interactions. This paper presents a detailed review on the developments in microcurrent-based computational frameworks for high-resolution haptic feedback. This work also investigates the modern techniques for high resolution haptic rendering, such as the role of multi-channel simulation and computational modeling techniques. Furthermore, the applications of this technology, challenges, and future directions were elaborated.},
booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence and Digital Media},
pages = {104–110},
numpages = {7},
keywords = {Deep learning, Electrical stimulation, High resolution, Microcurrent, Multichannel, Reinforcement learning, Virtual reality, Virtual tactile kinesthesia},
location = {
},
series = {GADM '25}
}

@inproceedings{10.1145/3641554.3701873,
author = {Thorgeirsson, Sverrir and Ewen, Tracy and Su, Zhendong},
title = {What Can Computer Science Educators Learn From the Failures of Top-Down Pedagogy?},
year = {2025},
isbn = {9798400705311},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3641554.3701873},
doi = {10.1145/3641554.3701873},
abstract = {While educational researchers in various disciplines are grappling with how to develop policies and pedagogical approaches that address the use of generative artificial intelligence, the challenge is particularly complex in computer science education where the new technology is changing the core of the field. In this paper, we take a look at the pedagogy of other subjects with a longer history than computer science and a more extensive body of educational research to collect insights on how this challenge can be met. We begin by drawing from recent neurological research to find domains that share cognitive commonalities with computer programming and then build upon comparisons that others have made to literacy and mathematics education. We then consider how the "reading wars" and "math wars" have shaped these fields, which we see as conflicts between less effective top-down pedagogy and more effective bottom-up pedagogy, and reflect on what would be comparable approaches in teaching computing. We find that approaches that make heavy use of large language models without teaching fundamentals can be compared to the top-down pedagogy of reading and mathematics and are likely to be ineffective on their own. Therefore, we advise against the exclusive use of such approaches with novices. However, we also acknowledge that the social science surrounding computer science education is complex and that effectiveness only tells a part of the story, with other factors such as engagement, motivation and social dynamics also being important.},
booktitle = {Proceedings of the 56th ACM Technical Symposium on Computer Science Education V. 1},
pages = {1127–1133},
numpages = {7},
keywords = {bottom-up pedagogy, computer science education, generative artificial intelligence, large language models, literacy, math wars, phonics, position paper, reading, reading wars, top-down pedagogy, whole language},
location = {Pittsburgh, PA, USA},
series = {SIGCSETS 2025}
}

@inbook{10.1145/3728725.3728801,
author = {Wan, Xing and Johari, Juliana and Ruslan, Fazlina Ahmat},
title = {Transformed Variational Color Shift Algorithm for Interfering Text Image Recognition},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728801},
abstract = {One common data augmentation method used in text image recognition is color shift transformations. A widely used method is the Variational Color Shift (VCS), which can automatically predict a range of color shifts for each image. The algorithm has significant overfitting when applied to text images that are highly interfered with and contain noise, which hinders further improvements in text recognition accuracy. In this study, we submit a new transformed layer inside the VCS framework using cyclic boundary shifts over the text images. A new module, Transformed Variational Color Shift (TVCS) is proposed. This method integrates a generalized set of colors (the original dataset) in a manner that can transform the input dataset. The experimental results show that TVCS has superior recognition accuracy than VCS and has better robustness against interference. The algorithm proposed is innovative as the data augmentation of text images, especially in a noisy environment.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {486–490},
numpages = {5}
}

@inbook{10.1145/3728725.3728758,
author = {Li, Shuaicheng},
title = {EEG Emotion Recognition Based on TQWT-features and Multiscale Convolution Superimposed Transformer},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728758},
abstract = {Emotional recognition can help machines to capture the emotional state of human beings, which is an indispensable technology for human-computer interaction and the realization of artificial intelligence. Electroencephalogram (EEG) has been applied to emotion recognition due to its good temporal and spatial resolution. How to capture the correlation of long-distance time-series EEG signal data from both temporal and spatial dimensions and directly affect the effect of emotion recognition is a difficulty in the BCI research. Transformer model can extract long-term correlations of time-series signals. In this paper, a new electroencephalogram emotion recognition method based on spatiotemporal fusion Vision Transformer is proposed. The feature of EEG signals is reconstructed by the tunable Q-factor wavelet transform (TQWT) method. The spatiotemporal correlation between the acquisition channel and the temporal signal is established by position coding for the convolved features. The hybrid model of multiscale convolutional and Vision Transformer (MC-ViT) is utilized for feature extraction and emotion classification. The EEG emotion recognition method based on spatio-temporal fusion vision transformer proposed in this paper has achieved good EEG emotion classification effect on the SEED dataset, which outperforms state-of-the-art deep learning methods.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {213–218},
numpages = {6}
}

@inbook{10.1145/3728725.3728726,
author = {Zhang, Siyuan and Zhou, Wenjun and Wang, Yifan and Zhang, Quan and Peng, Bo},
title = {MIFNetwork: Multi-scale Information Fusion Network for Long Traffic Prediction},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728726},
abstract = {Traffic prediction is a critical task in the development of intelligent transportation systems (ITS) and smart cities. Current methods mainly rely on Spatial-Temporal Graph Neural Networks (STGNNs), which effectively capture spatial dependencies but require significant computational resources and limited to short-term forecasting. However, research on time-series data in the transportation field, particularly on sequence prediction across multiple time scales, remains underexplored. To leverage the multi-scale characteristics of time-series data, as well as its seasonal and periodic features, we introduce two solutions based on DLinear model: multi-scale decomposition and cross-scale information fusion, and propose a traffic prediction model named MIFNetwork. We conduct comparative experiments on several traffic datasets, selecting transformer-based models with strong time-series modeling capabilities for comparison. The results demonstrate that our model outperforms other approaches, especially in handling long time spans and complex trend changes.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {1–6},
numpages = {6}
}

@inproceedings{10.1145/3605468.3609775,
author = {Philbin, Carrie Anne},
title = {Impact of Generative AI on K-12 Students’ Perceptions of Computing: A Research Proposal},
year = {2023},
isbn = {9798400708510},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3605468.3609775},
doi = {10.1145/3605468.3609775},
abstract = {The rapid progress of generative artificial intelligence (AI) is fundamentally reshaping traditional perspectives on knowledge and skills, with profound implications for computing education. This necessitates a thorough examination of the relevance and timeliness of computing as a subject, especially for K-12 students who are making critical decisions about their future qualifications. This abstract proposes an empirical research study that aims to explore the effects of integrating generative AI in the creation of digital artefacts on K-12 students’ perceptions of the value of computing, as well as their understanding of ownership and achievement. Constructive discussions regarding the outlined approach are encouraged.},
booktitle = {Proceedings of the 18th WiPSCE Conference on Primary and Secondary Computing Education Research},
articleno = {28},
numpages = {2},
keywords = {Artificial Intelligence education, Creative computing, Generative AI, K-12 education, Student perceptions},
location = {Cambridge, United Kingdom},
series = {WiPSCE '23}
}

@inbook{10.1145/3728725.3728795,
author = {Zhou, Chenglin and Huang, Cheng and Luo, Xiangyu},
title = {Synthesis of Real-time Linear Dynamic Logic over Finite Traces},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728795},
abstract = {Reactive synthesis, or simply synthesis, is primarily used to automatically generate control-intensive programs. Applications of reactive synthesis span various domains, including robot motion planning, autonomous system control, and communication protocol development. This paper investigates the reactive synthesis for formulas expressed in Real-Time Linear Dynamic Logic over finite traces (RTLDLf). RTLDLf is more expressive than Linear Temporal Logic over finite traces (LTLf). While RTLDLf and Linear Dynamic Logic over finite traces (LDLf) share equivalent expressive power, RTLDLf offers greater simplicity in its logical constructs compared to LDLf. Existing synthesis methods for LDLf predominantly rely on automata-based approaches. In this paper, we propose an RTLDLf synthesis method based on temporal testers. We first translate an RTLDLf specification into a temporal tester, a non-deterministic transducer. The tester is then transformed into a graph game between the system and the environment. We finally propose a symbolic algorithm to solve the game to derive a winning strategy for the system player. The proposed method has two advantages: (1) high composability, enabling convenient and efficient synthesis of complex RTLDLf formulas, and (2) symbolic representation of the tester's transition relations and state space, which facilitates the use of efficient symbolic algorithms based on Ordered Binary Decision Diagrams.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {447–452},
numpages = {6}
}

@inproceedings{10.1145/3698038.3698563,
author = {Shi, Ziji and Li, Jialin and You, Yang},
title = {ParaGAN: A Scalable Distributed Training Framework for Generative Adversarial Networks},
year = {2024},
isbn = {9798400712869},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3698038.3698563},
doi = {10.1145/3698038.3698563},
abstract = {Recent advances in Generative Artificial Intelligence have fueled numerous applications, particularly those involving Generative Adversarial Networks (GANs), which are essential for synthesizing realistic photos and videos. However, efficiently training GANs remains a critical challenge due to their computationally intensive and numerically unstable nature. Existing methods often require days or even weeks for training, posing significant resource and time constraints.In this work, we introduce ParaGAN, a scalable distributed GAN training framework that leverages asynchronous training and an asymmetric optimization policy to accelerate GAN training. ParaGAN employs a congestion-aware data pipeline and hardware-aware layout transformation to enhance accelerator utilization, resulting in over 30% improvements in throughput. With ParaGAN, we reduce the training time of BigGAN from 15 days to 14 hours while achieving 91% scaling efficiency. Additionally, ParaGAN enables unprecedented high-resolution image generation using BigGAN.},
booktitle = {Proceedings of the 2024 ACM Symposium on Cloud Computing},
pages = {1032–1044},
numpages = {13},
keywords = {Distributed Training, Generative Adversarial Network},
location = {Redmond, WA, USA},
series = {SoCC '24}
}

@inbook{10.1145/3728725.3728736,
author = {Fang, Heng},
title = {ColKGC: Collaborative Enhancement using Large Language Model for Knowledge Graph Completion},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728736},
abstract = {Knowledge Graph Completion (KGC) aims to address incomplete facts in Knowledge Graphs (KGs). As a mainstream approach for KGC, text-based methods are limited in performance due to insufficient knowledge caused by constraints in fine-tuning data. To incorporate broader external knowledge, some researchers explored KGC approaches based on large language models (LLMs), but general-purpose LLMs lack direct awareness of knowledge graph domain knowledge, making it challenging for them to independently perform KGC tasks. To balance the dependence of text-based methods on knowledge graph domain knowledge and LLMs on external knowledge, we propose Collaborative Enhancement using Large Language Model for KGC (ColKGC). We define the prompt template to implement the interaction between the text-based model and the LLMs, and divide the knowledge graph completion process into two stages. In the concept enhancement stage, we supplement the fine-tuning data by interacting with the prompts of the LLMs to enrich the graph domain knowledge learned by the text-based model. In completion inference stage, we adopted the iterative interaction strategy, taking the text-based model output as the prompt and taking the interaction content of the previous stage as the context to enhance the inference, which improved the LLMs' understanding of the KGC task. Our experiments demonstrate that ColKGC achieves superior results across various standard knowledge graph benchmarks. Extensive experiments also bear out ColKGC's efectiveness in generated data and the iterative interaction framework in assisting LLMs reasoning.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {66–71},
numpages = {6}
}

@inproceedings{10.1145/3643834.3661555,
author = {Hwang, Angel Hsing-Chi and Siy, John Oliver and Shelby, Renee and Lentz, Alison},
title = {In Whose Voice?: Examining AI Agent Representation of People in Social Interaction through Generative Speech},
year = {2024},
isbn = {9798400705830},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3643834.3661555},
doi = {10.1145/3643834.3661555},
abstract = {As generative artificial intelligence (genAI) applications gain popularity, there is a dearth of research examining how applications may transform social interactions. One possible application set to transform social interactions is the use of generative speech to power AI agents that can realistically represent people. Our work examines the potential implications of AI agents representing individuals in human conversations ("agent representation") as a way to begin filling this research gap. We take a multi-method approach, conducting formative interviews with developers, a co-design workshop with designers, a harm analysis among researchers, and interviews with the general public. Both technologists and potential users worry adopting agent representations might harm the quality, trust, and autonomy of human communication. Potential users are particularly concerned that agent representations could undermine the value of social interaction and threaten individuals’ ability to control their image. To avoid such potential consequences, future genAI-powered agents and speech applications should take into account user-defined red lines when considering applying these technologies in social settings.},
booktitle = {Proceedings of the 2024 ACM Designing Interactive Systems Conference},
pages = {224–245},
numpages = {22},
keywords = {agent representation, generative artificial intelligence, generative speech, interpersonal communication, social interaction},
location = {Copenhagen, Denmark},
series = {DIS '24}
}

@inproceedings{10.1145/3706598.3713906,
author = {Tao, Hongyi and Vyas, Dhaval},
title = {"Housing Diversity Means Diverse Housing": Blending Generative AI into Speculative Design in Rural Co-Housing Communities},
year = {2025},
isbn = {9798400713941},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3706598.3713906},
doi = {10.1145/3706598.3713906},
abstract = {In response to various environmental and societal challenges, co-housing has emerged to support social cohesion, grassroots innovation and ecological regeneration. Co-housing communities typically have smaller personal spaces, closer neighbourly relationships, and engage in more mutually supportive sustainable practices. To understand such communities’ motivations and visions, we developed a speculative design tool that harnesses Generative Artificial Intelligence (GenAI) to facilitate the envisioning of alternative future scenarios that challenge prevailing values, beliefs, lifestyles, and ways of knowing in contemporary society. Within the context of co-housing communities, we conducted a participatory design study with participants in co-creating their future communities. This paper unpacks implications and also reflects on the co-design approach employing GenAI. Our main findings highlight that GenAI, as a catalyst for imagination, empowers individuals to create visualisations that pose questions through a plural and situated speculative discourse.},
booktitle = {Proceedings of the 2025 CHI Conference on Human Factors in Computing Systems},
articleno = {527},
numpages = {17},
keywords = {Generative AI, speculative design, co-housing, sustainability},
location = {
},
series = {CHI '25}
}

@inproceedings{10.1145/3678610.3678631,
author = {Robledo-Rella, V\'{\i}ctor and Toh, Bee-Yen},
title = {Artificial Intelligence in Physics Courses to Support Active Learning},
year = {2024},
isbn = {9798400716799},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3678610.3678631},
doi = {10.1145/3678610.3678631},
abstract = {The integration of generative artificial intelligence (AI), particularly Large Language Models (LLMs) like OpenAI's ChatGPT and Microsoft's Copilot, is transforming educational methodologies, including undergraduate physics courses for engineering students. Despite their potential, these LLMs typically rely on statistical learning methods and often exhibit algebraic inaccuracies in solving standard university-level physics problems. This study explores the use of LLMs in physics courses for N = 91 freshman engineering students over two academic terms (Spring and Fall 2023). Students engaged in AI-assisted activities to solve physics problems and were asked to identify and correct the errors made by the chatbot. The outcomes were compared with those from traditional teaching methods without AI involvement, and no significant difference in student learning gains was found. To assess the impact of AI tools in education, a more detailed approach using pre-test and post-test instruments&nbsp;with control and experimental groups is necessary. Survey results revealed, however, that AI-assisted sessions enhanced student engagement, problem-solving skills, and understanding of physics concepts. Students also indicated a strong preference for AI-assisted activities, citing increased motivation and a firm belief in the educational benefits of using these tools. Our findings suggest that well-designed AI interventions can effectively complement traditional instructional methods, especially when the LLMs are integrated with symbolic computational tools like WolframAlpha to improve their accuracy.},
booktitle = {Proceedings of the 2024 10th International Conference on E-Society, e-Learning and e-Technologies (ICSLT)},
pages = {68–75},
numpages = {8},
keywords = {ChatGPT, Copilot, Educational Innovation, Generative AI, Higher Education, Interactive Learning, Physics Education Research},
location = {
},
series = {ICSLT '24}
}

@inproceedings{10.1145/3613905.3651096,
author = {Lo, Priscilla Y.},
title = {An Autoethnographic Reflection of Prompting a Custom GPT Based on Oneself},
year = {2024},
isbn = {9798400703317},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3613905.3651096},
doi = {10.1145/3613905.3651096},
abstract = {What if you could have a chat with yourself? OpenAI’s introduction of custom GPTs in November 2023 provides an opportunity for non-technical users to create specialized generative artificial intelligence chatbots. Users can write prompts in plain language rather than code to instruct how the system should behave. What can one learn from using non-technical methods to develop a specific chatbot persona? To explore this, I conducted an autoethnography of my experience developing and interacting with a custom GPT based on myself. My findings include a discussion of my experiences throughout the process, and its impact on my personal introspection and understanding of prompt engineering. I summarize first-hand challenges and insights intended to inspire further discussion on the topic of generative AI and chatbots.},
booktitle = {Extended Abstracts of the CHI Conference on Human Factors in Computing Systems},
articleno = {40},
numpages = {9},
keywords = {autoethnography, chatbot, generative artificial intelligence, large language model, persona},
location = {Honolulu, HI, USA},
series = {CHI EA '24}
}

@inbook{10.1145/3728725.3728766,
author = {Liu, Sihan},
title = {VideoDreamBooth: Subject-Driven Video Editing with Guided Multi-Path Fusion},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728766},
abstract = {Diffusion models have gained popularity for generating content in various domains, including text, image, and video. In particular, Text-to-Image(T2I) models have emerged as an innovative tool for artistic expression. They also spurred growth in other associated areas, such as personalized subject generation and one-shot video editing. This paper presents VideoDreamBooth, a novel framework for real video editing with personalized subject. Inspired by the coarse-to-fine denoising nature of diffusion, we propose a multi-path fusion technique to preserve temporal consistency with original video. We also propose a metric (CLIP score ) to guide which denoising steps will conduct fusion. Our framework has proven to be effective and successful for video editing in various experimental settings.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {258–265},
numpages = {8}
}

@inbook{10.1145/3728725.3728727,
author = {Li, Hao and Zhao, Jiahui and Gu, Daixuan},
title = {HoloDiT: Exploring 3D Diffusion Transformer Models Using 2D Images},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728727},
abstract = {Recently, Transformer-based diffusion models have produced impressive results in 3D shape generation. However, at this stage, they can only be trained using 3D point cloud data to generate simple 3D shapes based on point cloud representations. Building on this, we propose HoloDiT. It uses a set of real-world 2D images and corresponding camera poses as input, generating 3D auxiliary samples that are fed into a Transformer-based 3D diffusion model for training in 3D shape generation. By training the rendering function to generate color and density, we can restore the color details of object surfaces. This enables HoloDiT to generate realistic 3D shapes from 2D images. We trained on the real-world CO3Dv2 dataset, and the generated results demonstrate competitive quality and consistency compared to existing 3D generation models.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {7–11},
numpages = {5}
}

@inproceedings{10.1145/3579168.3632724,
author = {Ho, Shuyuan Mary and Liu, Yue},
title = {Genie Breaks the Bottle: Ethics in Artificial Intelligence Adoption: ChatGPT; The Beginning and the End of Human Wisdom},
year = {2024},
isbn = {9798400700941},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3579168.3632724},
doi = {10.1145/3579168.3632724},
abstract = {ChatGPT, developed by OpenAI, has attracted significant attention from early adopters. While this generative artificial intelligence (AI) system can be somewhat intuitive, such technology can also be disruptive in domains that require creativity (e.g., computer coding, system development and education), information authenticity (e.g., news agencies) and precision (e.g., manufacturing, clinical decision making). This study urges scholars in the fields of human-computer interaction and information system to reexamine technology adoption to better understand the criticality and ethics of AI and ChatGPT with regards to social change and social impact.},
booktitle = {Proceedings of the 2023 Computers and People Research Conference},
articleno = {6},
numpages = {4},
keywords = {Artificial intelligence, ChatGPT, deepfakes, generative adversarial networks, information ethics, social change},
location = {Pomona, CA, USA},
series = {SIGMIS-CPR '23}
}

@inbook{10.1145/3728725.3728786,
author = {Tian, Jihao and Wei, Yan},
title = {Sleep Spindle Auto Detection Based on U-Net with Label Optimization Module},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728786},
abstract = {The automatic spindle detection algorithm based on deep learning technology requires high consistency of sleep spindle annotation, which crowdsourcing can significantly enhance through collective intelligence. To best of our knowledge, few studies have explored auto detection of sleep spindles based on crowdsourced datasets. MODA is the only crowdsourcing spindle annotation open access dataset until now, and this study utilized the MODA dataset to further enhance the automatic detection of sleep spindles using conventional neural network (CNN) and innovative label optimization module. To evaluate our model, we proposed and conducted two analysis methods including analysis by sample-event and by subject. Meanwhile, to validate our performance of our model, SUMO and EXP were considered as control and gold standard methods respectively; and various metrics related to by sample-event and by subject were compared among our model, SUMO and EXP. The results from analysis by sample-event showed that F1 score, Matthew's correlation coefficient (MCC), and Cohen's Kappa coefficient in our model were significantly higher than in SUMO under all overlap thresholds. Specifically, under a higher threshold of 0.6, F1, MCC and Kappa reached 0.82, 0.81, 0.81 in our model, 0.75, 0.74, 0.74 in SUMO, respectively. For analysis by subject, all spindle features extraction of our model were comparable to the EXP, including density (4.48 in our model vs 3.80 in EXP), duration (0.79 vs 0.84), amplitude (29.91 vs 30.09) and frequency (13.04 vs 12.92). However, the spindle density extracted by SUMO was significantly different from EXP (9.04 in SUMO vs 3.80 in EXP). In conclusion, our results suggested that the crowdsourced public dataset and label optimization module can further enhance the spindle detection capability of the convolutional neural network.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {382–391},
numpages = {10}
}

@inbook{10.1145/3728725.3728747,
author = {Li, Fengdi},
title = {MH-PEFT: Mitigating Hallucinations in Large Vision-Language Models through the PEFT Method},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728747},
abstract = {Large vision-language models (LVLMs) have achieved significant progress and been widely applied across various industries. However, a series of significant challenges have also been posed, such as the hallucination, where the generated attributes of a target are inconsistent with the information in the image. Recent works have primarily focused on improving generation performance by training the multimodal alignment module of the model with additional data. However, this approach incurs higher costs and introduces redundant information due to the reliance on large amounts of additional data. To address these challenges, we propose a novel multimodal mitigation framework, termed MH-PEFT. Specifically, we introduce an effective data augmentation strategy that leverages existing data to generate additional attribute-related information, thereby enhancing the model's generative capabilities. Furthermore, we develop a PEFT-based training approach specifically tailored to mitigate multimodal hallucination in LVLMs. Notably, our framework features a plug-and-play design, which enhances its robustness and facilitates its deployment in real-world scenarios. Extensive experiments demonstrate that our method achieves state-of-the-art performance across multiple benchmark datasets.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {137–142},
numpages = {6}
}

@inbook{10.1145/3728725.3728797,
author = {Zuo, Mengxue},
title = {Infringement and Protection in Personal Information on Data Mining},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728797},
abstract = {Different from the personal information processing stage such as collection and analysis, the infringement of personal information by Data Mining is based on the mining process and mining results. On the basis of analyzing the operation mechanism and technical characteristics of Data Mining, there are four forms of infringement of personal information Data Mining. Specific manifestations are as follows: manifested as data labeling and unfavorable evaluation, information cocoons and passive autonomy, social sorting and data discrimination, information association and personal privacy infringement. The automatic decision-making and black box of Data Mining, the constructiveness and hiding of mining infringement behaviors, and the difficulty and challenge of infringement results pose the challenges and shortcomings of the current infringement law of personal information Data Mining in China. Finally puts forward the path to improve the legal protection of personal information, that is introducing the rules of due process, restricting the data power of Data Mining organizations, and establishing channels and procedures for effective interaction between Data Mining organizations and subjects of personal information.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {459–465},
numpages = {7}
}

@inbook{10.1145/3728725.3728751,
author = {Feng, Lei and Yu, Jiong and Li, Shu and Jiu, Shicheng},
title = {VAE-SMOTE Augmented Diffusion for Anomaly Detection},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728751},
abstract = {Most current generative models for anomaly detection, which rely on reconstruction methods, primarily use either Variational Autoencoders or Generative Adversarial Networks. However, VAEs tend to suffer from poor reconstruction quality, while GANs face challenges related to training stability and limited pattern coverage. Additionally, due to privacy and security concerns, the available training data for anomaly detection is often limited, whereas generative models typically require large datasets to avoid the risk of overfitting. To overcome these challenges, we introduce an unsupervised anomaly detection model utilizing diffusion techniques, designed specifically for tabular data, using a reconstruction-based approach. We adopt the diffusion model as the backbone for anomaly detection, aiming to enhance reconstruction quality and ensure training stability. To further improve reconstruction quality and accelerate inference, we employ a truncated diffusion strategy during inference. What's more, to overcome the challenge of insufficient training data, we introduce a SMOTE-based data augmentation method optimized for feature space in tabular data, incorporating a VAE to address the limitations of the original SMOTE algorithm in capturing complex data structures and handling nonlinear relationships. Experimental results on real-world datasets validate that our method achieves higher detection accuracy compared to conventional methods, demonstrating its effectiveness.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {163–170},
numpages = {8}
}

@inbook{10.1145/3728725.3728735,
author = {Liu, Tingting and Li, Rongbin and Yin, Jun},
title = {Color image authentication and encryption algorithm based on dual random phase encoding and dual chaotic system},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728735},
abstract = {Initially, an improved piecewise Logistic chaotic map is proposed, which is combined with a novel four-dimensional hyperchaotic system to form a dual-chaos system. By introducing dual random phase encoding and leveraging the dual-chaotic system along with Arnold transformation, the authentication image is encrypted. The authentication and encrypted images are combined, and the dual-chaos system along with DNA dynamic encoding and computation methods are employed to perform encryption and decryption on the combined image, thereby achieving both image encryption and authentication. In the performance analysis of the encrypted images, the algorithm proposed in this paper not only provides authentication functionality but also demonstrates strong security, with advantages such as high decryption quality and fast encryption speed. Furthermore, its authentication feature makes it suitable for most encryption scenarios.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {58–65},
numpages = {8}
}

@inproceedings{10.1145/3706598.3713762,
author = {Choi, Youjin and Moon, JaeYoung and Yoo, JinYoung and Hong, Jin-Hyuk},
title = {Understanding the Potentials and Limitations of Prompt-based Music Generative AI},
year = {2025},
isbn = {9798400713941},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3706598.3713762},
doi = {10.1145/3706598.3713762},
abstract = {Prompt-based music generative artificial intelligence (GenAI) offers an efficient way to engage in music creation through language. However, it faces limitations in conveying artistic intent with language alone, highlighting the need for more research on AI-creator interactions. This study evaluates three different interaction modes (prompt-based, preset-based, and motif-based) of commercialized music AI toots with 17 participants of varying musical expertise to examine how prompt-based GenAI can improve creative intention. Our findings revealed that user groups preferred prompt-based music GenAI for distinct purposes: experts used it to validate musical concepts, novices to generate reference samples, and nonprofessionals to transform abstract ideas into musical compositions. We identified its potential for enhancing compositional efficiency and creativity through intuitive interaction, while also noting limitations in handling temporal and musical nuances solely through prompts.  Based on these insights, we present design guidelines to ensure users can effectively engage in the creative process, considering their musical expertise.},
booktitle = {Proceedings of the 2025 CHI Conference on Human Factors in Computing Systems},
articleno = {766},
numpages = {15},
keywords = {Music generative AI, Prompt-based music generative AI, Composition experience, Creative process},
location = {
},
series = {CHI '25}
}

@inbook{10.1145/3728725.3728820,
author = {Chen, Xunlin and Li, Xiaoju and Chen, Ziyong},
title = {Research on Weed Identification Based on Deep Learning},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728820},
abstract = {In the intricate environment where crops and weeds grow intermingled, an effective solution to this problem is to precisely spray herbicides on weeds while accurately avoiding crops. The technical reliance in this process is the focus of this paper, which involves classifying, identifying, and locating weeds and crops. Currently, deep learning is widely used for image recognition and classification. However, the complex characteristics of the intermingled growth of weeds and crops are the main factors affecting the extraction of weed features. Based on an extensive review of domestic and foreign literature, and combining the research progress and methods of deep learning in the field of image recognition, this paper designs a method that can identify weeds with complex characteristics, effectively improving the accuracy of weed image recognition. This provides a favorable reference for the rapid and precise targeted spraying of herbicides in agricultural intelligent equipment or other devices beneficial to human life.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {605–609},
numpages = {5}
}

@inbook{10.1145/3728725.3728790,
author = {Zhao, Jiahui and Li, Hao and Tang, Suqin},
title = {A Hybrid Framework for Predicting Citation Harm from Retracted Papers},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728790},
abstract = {The increasing prevalence of retractions poses significant challenges to academic integrity and scientific progress. While various tools track retracted papers, the academic community lacks proactive mechanisms to predict the potential harm caused by citing retracted research. Building upon recent work quantifying the harm, we construct a dataset to forecast the harm papers will suffer from citing retracted research. We further propose a hybrid framework that combines language model with gradient boosted trees. Our framework achieves the lowest mean absolute error (MAE) across all prediction time horizons, achieving a 2.4% reduction in MAE compared to baseline methods. This work provides foundational resources and methodological insights for future studies on retraction impacts, aiming to help researchers make more informed citation decisions.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {411–416},
numpages = {6}
}

@proceedings{10.1145/3643690,
title = {IWSiB '24: Proceedings of the 7th ACM/IEEE International Workshop on Software-intensive Business},
year = {2024},
isbn = {9798400705717},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
abstract = {The workshop brings together research communities working on softwareintensive business and software engineering. It aims to bridge the gap between the research in these areas. This year's theme, "Software Business in the Era of Generative Artificial Intelligence," reflects our focus on exploring how generative artificial intelligence (GenAI) and the related large language models (LLMs) impact the established practices of software engineering and software business.},
location = {Lisbon, Portugal}
}

@inbook{10.1145/3728725.3728835,
author = {Lv, Hao},
title = {Aircraft Detection Based on Improved DETR},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728835},
abstract = {With the wide application of deep learning in computer vision, Transformer-based target detection methods, especially DETR, have become a research hotspot. However, DETR still faces low detection accuracy and efficiency problems when dealing with small objects and complex backgrounds. In order to enhance the performance of DETR in aircraft detection, this paper proposes an aircraft detection method based on an improved DETR architecture, which mainly consists of two innovative improvements: first, an ECA attention mechanism is introduced after CNN feature extraction to enhance the model's adaptive learning ability for important features; second, Swin Transformer is used to replace the encoder in the original DETR -decoder structure in the original DETR, using its local windowing self-attention mechanism to improve the computational efficiency and accuracy of the model. The experimental results show that the method proposed in this paper improves the detection accuracy and speed compared with the traditional DETR in the aircraft detection task, especially in the complex background with better robustness and generalization ability. The research in this paper provides an effective improvement idea for the Transformer-based target detection method and provides new technical support for high-precision target detection tasks in practical applications.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {691–697},
numpages = {7}
}

@inbook{10.1145/3728725.3728794,
author = {Chen, Jian and Li, Guangao and Wang, Jun and Sun, Siluo and Sun, Shoudao and Zhao, Yuntao},
title = {CBAM-EANet: Convolutional Block Attention Module Enhanced Efficient Network for Detection on Distributed Denial of Service Attacks},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728794},
abstract = {With the emergence of numerous DDoS attack methods, traditional detection techniques can no longer accurately and swiftly detect DDoS attacks. Furthermore, deep learning-based traffic image classification models often become overly complex in their pursuit of higher accuracy. In the paper, we propose a deep learning model-based method for the visual detection of DDoS malicious traffic. In response to the issue of model complexity, this article has made improvements based on the EfficientNet model, especially in resource constrained environments. This article introduces CBAM into the original model to improve its ability to process global information. By replacing the loss function in the final Softmax layer of the EfficientNet model, the ability to extract image features for binary classification problems has been enhanced, resulting in improved model performance. The experimental results show that this method can effectively identify various DDoS attacks. Compared with the EfficientNet model, this method has improved accuracy, training time, and single image prediction time.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {440–446},
numpages = {7}
}

@article{10.1145/3718098,
author = {Shi, Wenda and Wong, Waikeung and Zou, Xingxing},
title = {Generative AI in Fashion: Overview},
year = {2025},
issue_date = {August 2025},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {16},
number = {4},
issn = {2157-6904},
url = {https://doi.org/10.1145/3718098},
doi = {10.1145/3718098},
abstract = {Generative Artificial Intelligence (GenAI) has recently gained immense popularity by offering various applications for generating high-quality and aesthetically pleasing content of image, 3D, and video data format. The innovative GenAI solutions have shifted paradigms across various design-related industries, particularly fashion. In this article, we explore the incorporation of GenAI into fashion-related tasks and applications. Our examination encompasses a thorough review of more than 470 research papers and an in-depth analysis of over 300 applications, focusing on their contributions to the field. These contributions are identified as 13 tasks within four categories: multi-modal fashion understanding, and fashion synthesis of image, 3D, and dynamic (video and animatable 3D) formats We delve into these methods, recognizing their potential to propel future endeavors toward achieving state-of-the-art performance. Furthermore, we present a comprehensive overview of 53 publicly available datasets suitable for training and benchmarking fashion-centric models, accompanied by the relevant evaluation metrics. Finally, we review real-world applications, unveiling existing challenges and future directions. With comprehensive investigation and in-depth analysis, this article is targeted to serve as a useful resource for understanding the current landscape of GenAI in fashion, paving the way for future innovations in this dynamic field. Papers discussed in this article, along with public code and datasets links are available at: .},
journal = {ACM Trans. Intell. Syst. Technol.},
month = jun,
articleno = {74},
numpages = {73}
}

@inbook{10.1145/3728725.3728784,
author = {Li, Wang},
title = {Improvements to the Apriori Algorithm and Its Application in Educational Decision-Making Systems},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728784},
abstract = {The Apriori algorithm is among the traditional algorithms that are mainly employed to mine the educational data. Regrettably, the Apriori algorithm is not only the most classical algorithm, but also has some weaknesses, for instance, it has high computational complexity, generates too many candidate itemsets, which in turn leads it to be mainly used in small-scale educational data scenarios only. In this paper, the authors propose a modified version of the Apriori algorithm which aims to the rule generation process and the computational operation more efficient through data preprocessing phase information improvement and using the dynamic thresholding adjusting component as well as a not-so-well-known intelligent technique. The usefulness of the improved algorithm is tested by an experiment with actual course performance assessments, and the algorithm is also applied in the educational decision support system which comprises the course recommendation, teaching strategy optimization, and academic risk prediction the students. The results of the study suggest that there are no gains in script execution speed, generalization rate, or memory usage while there are with great advantage in the run time of the proposed algorithm. Consequently, the proposed algorithm is superior to the traditional one as it provides higher efficiency and thus, could be the best choice for educational data mining.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {367–373},
numpages = {7}
}

@inbook{10.1145/3728725.3728740,
author = {Han, Zijian and Hou, Changbo},
title = {Multimodal Knowledge Graph Inference Method Based on Cross-Attention Mechanism},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728740},
abstract = {Existing multimodal knowledge graph reasoning methods often rely on simple weighted averaging or attention mechanisms for multimodal feature fusion, which frequently results in the loss of modality-specific information and fails to fully exploit the complementary relationships between modalities. To address these issues, this paper proposes a multimodal knowledge graph reasoning method based on a cross-attention mechanism. The model employs the BERT model to encode textual information, introduces a VGG-16 model with a multi-scale attention module to encode visual information, and utilizes a graph attention mechanism to extract the structural information of the knowledge graph. Additionally, a contrastive learning framework is introduced to reduce semantic inconsistencies between modalities, and a cross-attention mechanism is used to achieve dynamic feature fusion across modalities. To validate the effectiveness of the proposed model, comparative experiments were conducted on the FB15k-237 and DB15K datasets. The results show that the proposed model significantly outperforms existing benchmark models on key evaluation metrics such as MRR, Hit@1, Hit@3, and Hit@10, demonstrating its effectiveness and robustness in multimodal knowledge graph reasoning.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {94–100},
numpages = {7}
}

@inbook{10.1145/3728725.3728748,
author = {Bao, Qiaozhi and Chen, Yanlin and Ji, Xusheng},
title = {Research on evolution and early warning model of network public opinion based on online Latent Dirichlet distribution model and BP neural network},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728748},
abstract = {Online public opinion is increasingly becoming a significant factor affecting the stability of the internet and society, particularly as the frequency of online public opinion crises has risen in recent years. Enhancing the capability for early warning of online public opinion crises is urgent. The most effective approach is to identify potential crises in their early stages and implement corresponding management measures. This study establishes a preliminary indicator system for online public opinion early warning, based on the principles of indicator system construction and the characteristics and evolution patterns of online public opinion. Subsequently, data-driven methodologies were employed to collect and preprocess public opinion indicator data. Utilizing grey relational analysis and the K-Means clustering algorithm, we classified online public opinion events into three levels: slight, warning, and severe. Furthermore, we constructed an online topic evolution model using the online Hierarchical Dirichlet Process model to analyze the thematic changes of online public opinion events across different warning levels. Finally, we developed an online public opinion early warning model using a Backpropagation (BP) neural network. The test results of early warning samples show that the model achieves high accuracy. Thus, in practical early warning applications, the BP neural network can be effectively utilized for predicting online public opinion events.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {143–151},
numpages = {9}
}

@article{10.1145/3689372,
author = {Jaidka, Kokil and Chen, Tsuhan and Chesterman, Simon and Hsu, Wynne and Kan, Min-Yen and Kankanhalli, Mohan and Lee, Mong Li and Seres, Gyula and Sim, Terence and Taeihagh, Araz and Tung, Anthony and Xiao, Xiaokui and Yue, Audrey},
title = {Misinformation, Disinformation, and Generative AI: Implications for Perception and Policy},
year = {2025},
issue_date = {March 2025},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {6},
number = {1},
url = {https://doi.org/10.1145/3689372},
doi = {10.1145/3689372},
abstract = {The emergence of generative artificial intelligence (GenAI) has exacerbated the challenges of misinformation, disinformation, and mal-information (MDM) within digital ecosystems. These multi-faceted challenges demand a re-evaluation of the digital information lifecycle and a deep understanding of its social impact. An interdisciplinary strategy integrating insights from technology, social sciences, and policy analysis is crucial to address these issues effectively. This article introduces a three-tiered framework to scrutinize the lifecycle of GenAI-driven content from creation to consumption, emphasizing the consumer perspective. We examine the dynamics of consumer behavior that drive interactions with MDM, pinpoints vulnerabilities in the information dissemination process, and advocates for adaptive, evidence-based policies. Our interdisciplinary methodology aims to bolster information integrity and fortify public trust, equipping digital societies to manage the complexities of GenAI and proactively address the evolving challenges of digital misinformation. We conclude by discussing how GenAI can be leveraged to combat MDM, thereby creating a reflective cycle of technological advancement and mitigation.},
journal = {Digit. Gov.: Res. Pract.},
month = feb,
articleno = {11},
numpages = {15},
keywords = {Misinformation, disinformation, trust, resilience, generative AI, social media}
}

@inbook{10.1145/3728725.3728813,
author = {Cai, Xinyu and Dai, Wenbo and Lu, Jingyu},
title = {Loan Default Prediction Based on Machine Learning Approaches},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728813},
abstract = {To address the credit risk losses incurred by commercial banks due to loan defaults, this study utilizes the loan default prediction dataset from the Alibaba Tianchi platform to develop machine learning models for predicting customer defaults, aiming to mitigate credit risk. Given the characteristics of class imbalance and high dimensionality of loan data, data preprocessing and exploratory data analysis are conducted. Based on a comparative analysis of various models, seven machine learning algorithms that demonstrate superior performance are selected for experimental comparison, including Decision Tree, Random Forest, AdaBoost, Bagging, XGBoost, LightGBM, and CatBoost. The results indicate that ensemble learning algorithms exhibit higher accuracy and predictive performance compared to single algorithms, with the CatBoost model performing best across various indicators, including AUC. The study identifies key features highly correlated with loan defaults, including loan grade, annual income, loan amount, credit history length, and debt-to-income ratio.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {557–564},
numpages = {8}
}

@inbook{10.1145/3728725.3728834,
author = {Yuan, Yufeng and Han, Peng and Qiu, Jian and Liu, Dongmei and Peng, Li and Chen, Miao and Luo, Kaiqing},
title = {SLP-SLAM: A Multifeature Fusion Stereo SLAM Based on Structural Lines and Structural Planes},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728834},
abstract = {Accurate positioning and pose estimation of mobile robots in structured environments have persistently presented considerable challenges. This paper proposes SLP-SLAM, a multifeature fusion stereo simultaneous localization and mapping (SLAM) system that combines both structural lines and structural planes. We have enhanced the effectiveness of the method for extracting structural lines using the Manhattan World Assumption (MWA) and have suggested utilizing these lines to construct structural planes, which is different from the existing plane extraction methods. Furthermore, we develop the state estimator by minimizing the visual residuals of points, structural lines, and structural planes. As far as we know, this represents the inaugural stereo SLAM that concurrently employs points, structural lines, and structural planes for integrated optimization. An appraisal of our system on difficult public datasets has been conducted. The trajectory error associated with SLP-SLAM has been diminished by 15% in comparison to contemporary state-of-the-art (SOTA) SLAM.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {685–690},
numpages = {6}
}

@inbook{10.1145/3728725.3728809,
author = {Wang, Jie and Tong, Xinyu and Ma, Xiaoya and Kong, Mengying and Zhang, Haifei and Yuan, Hailei},
title = {A recommendation algorithm model based on the fusion of group behavior and temporal feature},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728809},
abstract = {With the rapid development of the Internet and the exponential growth of network information resources, how to help users quickly obtain valuable information has become a major challenge. As the key technology of this problem, recommendation algorithm has become an important tool in the information age. However, most existing recommendation systems, such as traditional deep learning recommendation algorithms, still have problems such as cold start of new users and improper handling of dynamic changes in user behavior. To solve these problems, this paper proposes a fusion Group behavior characteristics and time series characteristics of the recommendation algorithm model Group-Temporal Recommendation Fusion (GTRF). Based on deep learning algorithm, the model extracts group behavioral characteristics by using graph attention network to deal with the problem of cold start, obtains time series characteristics by using gated convolution and GRU to deal with the problem of dynamic changes in user behavior, and finally uses cross-attention mechanism to effectively integrate these two types of features to achieve high-precision prediction of recommended content. GTRF not only solves the problems such as cold start of new users and dynamic change of user behavior, but also significantly improves the comprehensive performance of recommendation algorithm, showing the indispensable and great potential of recommendation algorithm in information retrieval.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {532–537},
numpages = {6}
}

@inproceedings{10.1145/3728725.3728770,
author = {Liu, Quan and Song, Yunkui},
title = {Optimization of Price Strategy by Machine Learning in E-commerce},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728770},
doi = {10.1145/3728725.3728770},
abstract = {Machine learning algorithms can monitor market dynamics, user behavior and competitor prices in real time, and dynamically adjust commodity prices based on this information to maximize profits. By analyzing historical sales data, market price data, user behavior data, etc., machine learning models can predict the demand and price sensitivity of goods to develop optimal pricing strategies. This study takes Pinduoduo and Taobao as examples to analyze the application of machine learning in E-commerce price strategy optimization, in order to provide useful reference for the E-commerce platforms.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {284–288},
numpages = {5},
keywords = {E-commerce, Machine learning, Pinduoduo, Price strategy, Taobao},
location = {
},
series = {GAIIS '25}
}

@inbook{10.1145/3728725.3728765,
author = {Zhao, Weidong and Jiang, Xuan and Qiu, Liqing},
title = {WWR-CS: Chinese Spelling Error Correction Based on Whole Words Replacement fusing Confusion Set},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728765},
abstract = {Chinese Spelling Error Correction, which aims to detect and correct spelling mistakes in Chinese text, is an essential branch of Natural Language Processing (NLP) field. However, the similarity of character sounds and shapes poses considerable challenges in Chinese spelling correction. Although significant progress has been made in the BERT-based Masked Language Models (MLMs) incorporating phonological and morphological information, further research is needed on improving correction accuracy in multiple error scenarios. Therefore, the paper proposes the Chinese Error Correction based on Whole Words Replacement fusing Confusion Set (WWR-CS) model which utilizes the whole words replacement method, incorporating confusion set in the noise generation module. Furthermore, due to the fact that most spelling errors stem from phonetic misuse, the model incorporates the pronunciation of Chinese characters and comprehensively considers character correction and pinyin prediction to obtain the final correction result. The experimental results on the Sighan datasets exhibits improved performance compared to the models proposed in recent years, verifying the effectiveness of the proposed model.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {251–257},
numpages = {7}
}

@inbook{10.1145/3728725.3728752,
author = {Xiong, Botao},
title = {Audio-driven Talking-face Synthesis based on 3D Gaussian},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728752},
abstract = {Audio-driven talking-face synthesis is of significant importance in various application scenarios, such as remote meetings, AR/VR, and digital humans. Currently, the work in this field can be broadly divided into two categories: implicit representation-based methods and explicit representation-based methods. Implicit representation-based methods often use neural network models to represent human face. For example, NeRF (Neural Radiance Field) uses MLP (Multi-Layer Perceptron) to represent human face, and by inputting the camera pose, NeRF renders a face image from that viewpoint. When training with audio features, it can render face images with different facial movements. However, due to the lack of editability and slow rendering speed of implicit representations, the industry has been exploring new representation methods. Explicit representation-based methods commonly use 3D Gaussian representations of the face, which offers high editability and nearly real-time rendering speed, making these methods surpass implicit-based approaches in many metrics. However, it is difficult to incorporate other signals during the rendering optimization process. To address this issue, this paper combines 3D Gaussian representations with the 3D head model FLAME (Faces Learned with an Articulated Model and Expressions). The FLAME model represents the face's shape and facial movements with a set of parameters. In this paper, audio signals are first transformed into parameters representing facial movements, which are used to initialize the 3D model generated by the parameters, followed by rendering optimization. This paper proposes a 3D Gaussian-based audio-driven lip-sync video generation system. It consists of two main modules: the audio-to-facial movement module and the 3D Gaussian rendering module. The audio-to-facial movement module uses the extracted audio features to generate parameters that control the FLAME model, converting the audio input into natural and reasonable lip shapes and facial movements. The 3D Gaussian rendering module initializes the 3D Gaussian with the FLAME model, optimizes the rendered image from coarse to fine, and finally stitches the generated images together to create a continuous video. Experiments show that this system can generate lip-synchronized, accurate, high-fidelity videos.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {171–177},
numpages = {7}
}

@inbook{10.1145/3728725.3728791,
author = {Cao, Yu and Du, Xuehui and Zhang, Lulu and Wang, Shihao},
title = {An Efficient Zero-Knowledge Range Proof Scheme Based on Multibit Split Square Decomposition},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728791},
abstract = {With the continuous development of blockchain technology, an increasing number of scholars have begun to consider the harm of data leakage during on-chain transactions and the requirement for privacy data protection. Zero-knowledge range proof, as a cryptographic technology, can perform legitimacy verification of data while hiding private data, effectively realizing the protection of private data on the blockchain, so it is increasingly used to protect blockchain privacy. The mainstream construction methods for range proofs can be mainly divided into two categories: n-ary decomposition and square decomposition. This paper introduces and analyzes the advantages and disadvantages of these construction methods in detail. Then, based on these two methods, a zero-knowledge range proof scheme based on multibit split square decomposition (ZKRPMSSD) is proposed, which requires no trusted third-party setting and can achieve range proofs for arbitrary ranges. The proposed ZKRPMSSD scheme processes the original data based on the multibit split idea, and the acquisition method of secret value components is optimized so that the acquisition of components does not depend on the scale of the original problem. Additionally, the algorithms for proof generation and verification in the ZKRPMSSD scheme are redesigned based on the (Sigma ) protocol and Pedersen commitments, effectively reducing the computational cost of the proof generation and verification process. Finally, typical n-ary decomposition and square decomposition zero-knowledge range proof construction schemes are taken for comparative analysis. Under 256-bit security and the same problem scale, experimental results indicate that ZKRPMSSD has advantages in proof and verification time costs.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {417–426},
numpages = {10}
}

@inbook{10.1145/3728725.3728732,
author = {Guo, Penggang and Liu, Yunfan and Li, Longfei and Liu, Changyuan},
title = {Improved Semantic Segmentation of Urban Road Scenes with Deep Dual-Resolution Network},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728732},
abstract = {In response to the requirements for efficiency and accuracy in segmentation models in autonomous driving scenarios, an improved urban road semantic segmentation algorithm based on the Deep Dual-Resolution Network (DDRNet) has been proposed. Firstly, a Channel Residual Attention Module (CRAM) based on standard attention mechanisms is introduced at the end of two parallel branches of the DDRNet to focus on important channel features. Secondly, the backbone network incorporates Depthwise Over-parameterized Convolution (DO-Conv) to boost feature extraction performance. Finally, a Deep Aggregation Pyramid Pooling Module (DAPPM) with Hybrid Dilated Convolution (HDC) is designed to capture multi-scale feature information, and Group Normalization (GN) is utilized to mitigate the impact of reduced batch size. The experimental results demonstrate that the proposed method achieves a Mean Intersection over Union (MIoU) of 77.0% on the Cityscapes dataset, representing an improvement of 1.1% over the baseline model. Additionally, the segmentation speed reaches 106 Frames Per Second (FPS), achieving an optimal balance between segmentation accuracy and computational efficiency.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {37–42},
numpages = {6}
}

@inbook{10.1145/3728725.3728767,
author = {Jiang, Nan and Liu, Chenyu and Yu, Shaofei},
title = {Research on Deep Data Annotation Methods Based on Expertise in the Power Industry},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728767},
abstract = {This study presents the development of an automated annotation tool for energy industries such as power, leveraging large language models to address challenges in processing unstructured text data efficiently and accurately. The tool comprises two main subsystems: data cleaning and preprocessing, and data generation. The data cleaning subsystem normalizes multi-format documents, converting them into structured formats suitable for annotation. The data generation subsystem, based on a large language model, includes modules for text, table, terminology, and image annotation, enabling the automatic generation of diverse question-answer pairs. The tool's performance was evaluated on 574 documents of eight common types of documents. Results show the tool's strong annotation capability, achieving high accuracy and coverage rates. However, limitations were observed in areas such as term recognition, context understanding, and multimodal content handling, highlighting avenues for future improvement. This automated annotation tool offers a scalable, cost-effective solution for data annotation in the power industry and lays the groundwork for enhancing data-driven decision-making and knowledge extraction across similar industries.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {266–271},
numpages = {6}
}

@inbook{10.1145/3728725.3728829,
author = {Li, Yang and Chen, Chi and Li, Yong},
title = {Pos-PCFG: A Password Guessing Method Incorporating Character Positions},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728829},
abstract = {Due to its easy-to-use characteristic, passwords are still the main way to authenticate a person online. However, as the increasing number of websites, users now often come across the issues of password overload. Such repeated use of one's passwords or choosing simple passwords such as names or dates could lead to serious security risks. Password-guessing simulations could effectively prevent these vulnerabilities. Nowadays, most modern websites request passwords with special characters to enhance their complexity. This study emphasizes the special characters within passwords. By using t-SNE visualization and KL divergence, we have analyzed the distribution of these characters among different passwords visually and quantitatively. As the result shows, special characters present a unique distribution mode in t-SNE visualization, with a higher KL divergence value than alphabetic and digit segments. Based on our findings, a password-guessing approach that classified the special character segments into four types according to the position named the Pos-PCFG model has been proposed. The model has improved the guessing accuracy of multiple datasets. In the Rockyou dataset test, accuracy was achieved with a notable 0.98% increase when guessing passwords consisting of special characters.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {659–666},
numpages = {8}
}

@inproceedings{10.1145/3728725.3728775,
author = {Zheng, Jiamin and Peng, Shijia and Zhou, Yipeng and Song, Shaoyi and Zhao, Xuan},
title = {Secure Sharing and Protection of Agricultural Data Based on Federated Learning},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728775},
doi = {10.1145/3728725.3728775},
abstract = {With the acceleration of agricultural digitization process, the security sharing of agricultural data has become a key issue to be solved urgently. This paper focuses on this, introduces the related technologies such as blockchain, federated learning and interplanetary file system, and proposes the design of agricultural data sharing model based on federated learning. The work flow of this scheme is described in detail and evaluated by experiment. The security analysis shows that the scheme can effectively resist various security threats and protect the privacy of agricultural data. The accuracy analysis shows that the training accuracy of the model is almost unaffected while the safety is improved. Homomorphic encryption analysis verifies the effectiveness of the encryption technology in the scheme. This scheme provides a feasible and efficient solution for data security sharing of agricultural intelligent knowledge service platform, and is expected to play an important role in related fields.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {312–319},
numpages = {8},
location = {
},
series = {GAIIS '25}
}

@inbook{10.1145/3728725.3728745,
author = {Fu, Lin},
title = {A Method for Discovering Ancient Chinese Neologisms Based on the Combination of Information Entropy - Mutual Information and BI-LSTM-CRF Model},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728745},
abstract = {Traditional new word discovery often relies on the information entropy-mutual information (IM) algorithm. In this study, we propose a joint IM algorithm and bidirectional long short-term memory conditional random field (BI-LSTM-CRF) neural network model, referred to as IM-BI-LSTM-CRF. This model retains the advantages of the IM algorithm while leveraging the BI-LSTM model to capture contextual dependencies and the CRF layer to model label dependencies. Using this hybrid approach, we conducted new word discovery experiments on a self-annotated dataset, utilizing word embedding vectors and different word position annotation sets. The results demonstrate that the IM-BI-LSTM-CRF model surpasses traditional LSTM and BI-LSTM models in new word discovery and exhibits strong generalization capabilities.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {125–131},
numpages = {7}
}

@inbook{10.1145/3728725.3728787,
author = {Huang, Zhiyu and Wu, Zhen},
title = {Malicious Traffic Identification Based on Composite Sampling},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728787},
abstract = {In response to the problem of insufficient attack traffic samples in network intrusion detection datasets, a hybrid sampling data augmentation method (S-VAE) that combines SMOTE and VAE is proposed. Firstly, train corresponding VAE models for different types of attack traffic samples; Secondly, the SMOTE algorithm is used to extract the original samples and their neighboring samples from the dataset and store them in pairs; Then, the paired samples are fed into the trained VAE model to learn their latent representations separately, and the latent representations of the original and neighboring samples are linearly interpolated; Finally, the results of linear interpolation are fed into the trained VAE decoder to expand the new samples. In this way, a balanced dataset can be established to enable deep learning models to fully learn the features of a small number of samples. The experimental results showed that using convolutional neural network models for multi classification experiments on the NSL-KDD and UNSW-NB15 datasets, S-VAE increased the classification recall by 3.75 and 3.06 percentage points, respectively, and the F1 score by 6.49 and 4.29 percentage points, respectively, compared to the baseline model.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {392–398},
numpages = {7}
}

@inbook{10.1145/3728725.3728788,
author = {Wang, Liangjie and Wang, Zhongyong and Zhai, Huipeng and Hu, Po and Wang, Wei},
title = {Spectrum anomaly detection and defense algorithm based on attention mechanism residual neural network},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728788},
abstract = {Spectrum anomaly detection involves identifying abnormal signals within the electromagnetic spectrum. Most existing methods focus on traditional abnormal signals and can achieve high detection rates through training and testing of neural network models. However, the presence of adversarial samples greatly reduces the performance of existing network models. To address this issue, a spectrum anomaly detection algorithm based on attention mechanism residual neural network is proposed, which can significantly improve the robustness of network models without adversarial training. This method integrates the attention mechanism module and the Residual Network (ResNet), using ResNet18 to process spectrogram data, avoiding gradient vanishing and overfitting during iteration, and using attention mechanism to enhance the model's attention to local information in spectrogram data. The experimental results show that the predictive performance of the proposed algorithm is superior to the compared algorithms. Compared with ResNet18 and feature transformation based image retrieval adversarial defense algorithms, the new algorithm has increased the AUC value for abnormal signal detection by approximately 47% and 24%, respectively, effectively improving the robustness of the network model and reducing the negative impact of adversarial samples on the neural network model. The experimental results verify the effectiveness and superiority of the residual neural network model based on attention mechanism.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {399–404},
numpages = {6}
}

@inbook{10.1145/3728725.3728828,
author = {Xu, Xintao},
title = {Exploration of Multi-modal Translation Resource Processing Modes under Computer Graphics: A Case Study of Visionbank},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728828},
abstract = {The rapid development of computer technology has brought computer graphics a qualitative leap in graphic images and visual communication. Computer graphics transforms digital information into image information while conducting digital processing and visualization of graphic images. Combining with the development process of computer graphics, this paper considers the application field of computer graphics, discusses the connotation deduction of multi-modal translation resource. Through case analysis of the Visionbank, this paper validates and advances computer graphics technology to optimize multi-modal translation resource, and realizes the comprehensive utilization of multi-modal translation resource.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {654–658},
numpages = {5}
}

@inbook{10.1145/3728725.3728827,
author = {Wang, Qiuyang and Chen, Sixi and Chen, Yening and Yan, Hong and Li, Jianwei},
title = {Research on Deep Learning Models for Dehazing Wildfire Images},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728827},
abstract = {In recent years, image dehazing tasks have primarily focused on urban or indoor scenarios, with little attention paid to the study of image dehazing in forest fire contexts. The degradation of images caused by the dense smoke generated by forest fires poses a significant obstacle to firefighting and rescue operations. Unlike ordinary haze, the smoke produced by forest fires is characterized by its non-homogeneous density, which complicates the process of smoke removal. Moreover, there is limited research dedicated to smoke removal specifically in wildfire environment images. To address this gap in image dehazing research for forest fire scenarios, this study trains and tests various specialized dehazing methods on a new public dataset for forest fire dehazing, Fo-haze. The methods are divided into four groups based on their network structures and training strategies. Through qualitative and quantitative evaluations of the models' performance, the study aims to assist firefighters in assessing the dehazing capabilities of different models. The experimental results indicate that the CHaIR model from the Technical University of Munich, Germany, achieved the best results, with a PSNR of 27.11 dB and an SSIM of 0.8721.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {648–653},
numpages = {6}
}

@inbook{10.1145/3728725.3728746,
author = {Yang, Rui and Chen, Hongqian and Gao, Yu and Ge, Yuting},
title = {EAM_DiffusionSeg: Research on Instance Segmentation Methods Based on Enhanced Aggregation Masks and Diffusion Models},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728746},
abstract = {In this paper, we propose an instance segmentation framework EAM_DiffusionSeg based on enhanced aggregation masks, to address the problems of incomplete instance detection and occlusion sensitivity in diffusion models for multi-target segmentation. To overcome the limitations of existing methods that rely on a single mask optimization path, this work innovatively constructs a dual-path mask generation mechanism: a dual-path mask generation mechanism is proposed: (1) a noise filter based on the diffusion model generates the structure-optimized mask M2(M_diffusion); (2) a mask prediction branch integrated into the local decoder outputsM1(M_local), which preserves finer details. and based on the spatial adaptive weighting, the augmented aggregation formula is designed to achieve the feature complementation. Experiments on the COCO dataset demonstrate that the proposed method significantly improves detection capability and boundary segmentation accuracy in occluded scenes, and provide a new solution for instance segmentation of complex scenes.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {132–136},
numpages = {5}
}

@inbook{10.1145/3728725.3728822,
author = {Liu, Shiqing},
title = {MGF-GNN: A Multi-Granularity Graph Fusion-based Graph Neural Network Method for Network Intrusion Detection},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728822},
abstract = {With the increasing complexity and diversity of network attacks, Graph Neural Networks (GNNs) have been employed to address network intrusion detection challenges. However, the performance of these models is constrained by their reliance on a single graph construction approach. To enhance detection accuracy and adaptability to intricate attack patterns, this paper introduces a Multi-Granularity Graph Fusion-based Graph Neural Network (MGF-GNN) model. The model resolves the issue of a single granularity graph structure failing to fully capture the complex relationships and covert attack strategies in network traffic. It constructs three heterogeneous graphs at different granularities to capture traffic relationships from host-level, port-level, and protocol-level perspectives. The model employs hierarchical message passing, aggregation, and cross-graph information fusion mechanisms to strengthen its ability to learn multi-level information, thereby improving detection performance against complex attacks. MGF-GNN outperforms traditional machine learning methods and single-graph-based GNN models on the CIC-IDS2017 and UNSW-NB15 datasets, achieving significant improvements across multiple evaluation metrics, including accuracy, precision, recall, and F1 score. Its effectiveness is primarily attributed to MGF-GNN's ability to comprehensively learn multi-dimensional perspective information through multi-granularity graph fusion.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {616–620},
numpages = {5}
}

@inbook{10.1145/3728725.3728730,
author = {Tang, Fukang and Liu, Changhua},
title = {AES encryption scheme based on Cubic chaotic mapping optimization},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728730},
abstract = {In order to improve the performance and security of information encryption system in big data scenarios, an AES encryption scheme based on chaos mapping optimization is proposed. Cubic chaotic mapping is studied and optimized, the randomness and unpredictability of chaotic sequence is enhanced by buffting optimization model, and the optimization effect is verified by Lyapunov index, bifurcation diagram, phase diagram and time series. By combining the optimized chaotic sequence with AES, a dynamic key generation mechanism is designed to avoid the security risks of static keys, and the performance of AES optimized keys is verified from the aspects of randomness, security, resource consumption and anti-attack ability. The experimental results show that the scheme significantly enhances the anti-attack capability and processing performance of the encryption system, and provides important support for the information security field.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {26–30},
numpages = {5}
}

@inbook{10.1145/3728725.3728734,
author = {Wang, Xiaoyu and Zhao, Weidong and Qiu, Liqing},
title = {Building Specialized Adversarial Examples for Improved Spelling Error Correction},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728734},
abstract = {Pre-trained language models based on deep neural networks have achieved superior performance in Chinese spelling correction. However, these models are heavily dependent on the size and quality of the training corpus and exhibit poor performance when faced with unknown errors. Inspired by adversarial learning, this paper focuses on Chinese character input scenarios and propose an effective attack strategy to generate more valuable training examples in these scenarios. This strategy jointly utilizes the attention mechanism and the logit output to determine the attack position and generates misspelling instances in a perturbed pinyin manner. The generated adversarial samples are then gradually added to the training set to train the model. Further, this paper explores the effect of the proportion of samples in the adversarial training process on model performance. The effectiveness of the proposed strategy are demonstrated on three different architectural baseline models.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {50–57},
numpages = {8}
}

@inbook{10.1145/3728725.3728824,
author = {Yang, Chenming},
title = {Three-Dimensional Path Planning for Unmanned Aerial Vehicles Based on a Hybrid Strategy of Dung Beetle Optimization Algorithm and Grey Wolf Optimization Algorithm},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728824},
abstract = {This paper presents a novel UAV three-dimensional path planning method by integrating the Dung Beetle Optimization (DBO) algorithm and the Grey Wolf Optimization (GWO) algorithm. Aiming at the problems of traditional algorithms in UAV path planning, such as local optima trapping and low convergence accuracy, this hybrid algorithm combines the advantages of both algorithms. The Fuch and Logistic chaotic maps are used for population initialization to enhance the global search ability. During the optimization process, the DBO algorithm's strong local exploitation ability and the GWO algorithm's global search characteristics are coordinated. A B\'{e}zier curve is employed to smooth the planned path. Simulation results demonstrate that the proposed hybrid algorithm outperforms traditional algorithms in terms of path length, convergence speed, and path smoothness, validating its effectiveness in UAV three-dimensional path planning.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {626–631},
numpages = {6}
}

@inproceedings{10.1145/3728725.3728774,
author = {Ye, Jianbing and Lin, Chaowen and Li, Qi},
title = {Line Detection Based on Hough Transform and its Application},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728774},
doi = {10.1145/3728725.3728774},
abstract = {Line detection is an important task in computer vision, which is widely used in fields such as automated driving and remote sensing image processing. As a classical image processing method, Hough transform has strong robustness and anti-interference ability in line detection. In this paper, line detection based on Hough transform is implemented and accelerated to hardware implementation on High-Level Synthesis (HLS) platform. By performing edge detection on the input image, the Hough transform maps the image from Cartesian space to parameter space to detect line features in the image. Experimental results show that the method can accurately detect lines in the image while exhibiting strong noise rejection. The implementation not only improves the efficiency of line detection, but also provides an effective solution for hardware acceleration applications based on the Hough transform.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {307–311},
numpages = {5},
keywords = {Edge Detection, Hardware Acceleration, High-Level Synthesis, Hough Transform, Line Detection},
location = {
},
series = {GAIIS '25}
}

@inbook{10.1145/3728725.3728742,
author = {Zhang, Xiaofeng and Gao, Xianming and Tao, Peilin and Feng, Tao},
title = {GraphSynth: Synthesis of Network Configuration Templates Using Large Language Models},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728742},
abstract = {We investigate whether Large Language Models can synthesize correct network configuration templates in a simpler and more efficient way with limited prompts. The experimental results demonstrate that the LLM's shortcomings in command relation understanding can be supplemented by external models to generate promising templates. We do not rule out the contribution of validators to property-specific consistency. However, our framework aims to preserve the large language model generation capability while focusing on the implicit relationships in configurations. By introducing a hierarchical command graph, we follow a top-down matching to generate templates, reducing feedback interactions in generation and bring potential solutions for automated provisioning. We process template generation under different scenarios, including establishing BGP basic connections on multiple routers and imposing restrictions such as route redistribution at specific locations. Human involvement is restricted to providing critical information. Experiment shows that expected templates can be generated in the first iteration, particularly for the basic task. The advanced task is influenced by multiple factors, however, it still shows considerable potential in terms of coverage with possibly feasible templates.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {108–114},
numpages = {7}
}

@inbook{10.1145/3728725.3728807,
author = {Wan, Guofeng},
title = {Research and Application of Generalized Predictive Control Algorithm in ATO System},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728807},
abstract = {Aiming at the safety guarantee of the train in the process of operation, whether it can arrive on time and comfort in the process of riding. In this paper, generalized predictive control algorithm is applied to automatic train operation system. First of all, the structure and function of the ATO system of the train are studied and analyzed, and then the force analysis of the train operation is carried out and the transformation of each working condition is understood and the situation is applicable. Secondly, the GPC algorithm is studied in detail, and its prediction model, rolling optimization and online identification are recognized respectively, and the train speed controller is designed. Finally, the ATO system is simulated and designed based on GPC algorithm, and the train model is analyzed. Moreover, it can be verified by simulation that under the GPC algorithm, the train can track the set target curve well during operation, ensuring the safety, comfort, energy saving and accuracy of the train operation.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {522–525},
numpages = {4}
}

@inbook{10.1145/3728725.3728744,
author = {Huang, Hui-Wen and Chang, Dong-Meau and Sankey, Andrew},
title = {Exploring EEG-based Attention Levels in Human-AI Interactions: A Case Study},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728744},
abstract = {This study used a non-invasive electroencephalogram (EEG) instrument to explore learner attention during interactions with AI chatbots. Compared with traditional self-reported surveys, EEG-based data offer a more direct and objective method for measuring cognitive engagement. To assess differences in attention levels before and after interacting with AI chatbots, the authors invited three Chinese college students to participate in this case study. The results indicated their attention levels increased during chatbot interactions, suggesting that personalized and interactive AI tools can help students stay focused. This case study contributes to the literature on observing cognitive engagement and offers educators a fresh strategy for improving students’ attention and academic performance.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {120–124},
numpages = {5}
}

@inbook{10.1145/3728725.3728814,
author = {Liu, Shani and Wang, Shan and Zhao, He},
title = {Realization and Experimental Research on Immersive Visual Communication System Based on Real-Time Rendering},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728814},
abstract = {Aiming at the problems of low image processing efficiency and insufficient rendering quality in virtual reality content creation, a real-time rendering system based on deep learning is designed and realized. An improved U-Net network structure is used for image processing, and a rendering speed of 112fps is realized in a standard test environment, with the end-to-end delay controlled within 12ms. The system achieves a task completion rate of 95.8% and a satisfaction score of 4.6 in a test with 1000 users. The experimental results show that the system significantly improves the rendering efficiency and interactive experience, and provides reliable technical support for virtual reality content creation.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {565–569},
numpages = {5}
}

@inbook{10.1145/3728725.3728749,
author = {Jin, Yujun and Luo, Tao},
title = {DPRS: Distributional Perspective Modeling for LLM-Enhanced Recommendation Systems},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728749},
abstract = {Existing recommender systems mainly rely on ID embedding to model user behavior sequences. However, this approach ignores the large amount of semantic information embedded in the textual descriptions of items, resulting in low performance. The ability to utilize large-scale language models to understand and reason about textual content offers a promising avenue for recommender system development. To this end, we propose a recommender system model, DPRS, which models textual representations of large-scale language models from a data distribution perspective and incorporates them into traditional recommendation models. We utilize pre-trained LLMs as item encoders to introduce textual representations to the recommender system. To address the heterogeneity between the LLM representations and the original recommender system, we propose to model user behavior sequences from a data distribution perspective by converting them into similarity score sequences. By conducting experiments on two public datasets, we verify the superiority of the proposed model.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {152–156},
numpages = {5}
}

@inbook{10.1145/3728725.3728728,
author = {Yang, Kai and Shi, Huaifeng and Wang, Rui and Zhou, Lei and Peng, Jiahui and Liu, Chaofan},
title = {Trend Decomposition Enhanced Large Language Model for Non-Stationary Network Traffic Prediction},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728728},
abstract = {Regular network traffic prediction techniques, which generally rely on data normalization, often fail to account for the non-stable characteristics of network traffic, leading to a decline in estimate reliability. A novel approach to this problem is the Trend Decomposition Enhanced Large Language Model for Non-Static Network Traffic Prediction (TD-LLM). Applying trend decomposition as the preparation approach, the proposed strategy first divides raw information into resolved and non-fixed parts. The predetermined portion is standardized and processed, implementing a multi-head cross-attention mechanism, while a better self-attention mechanism improves the non-stationary element. Prompt engineering is used to create specially designed causes that serve as a guideline for the design throughout the forecast process. Finally, the processed data and creates are integrated into the large language model to produce the predicted results. In contrast to foundation models like Time- LLM, LSTM, Informer, and Transformer, the TD-LLM type drastically reduces mean squared error for network traffic prediction across many regions, according to exploratory evaluations, with decreases of 19.73 %, 54.71 %, 76.88 %, and 61.48 %, respectively.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {12–19},
numpages = {8}
}

@inbook{10.1145/3728725.3728802,
author = {Zhang, Zheng and Xia, Jiewen and Yu, Yang},
title = {Fully distributed scaled consensus tracking of high-order unknown nonlinear multi-agent systems},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728802},
abstract = {An adaptive fast robust scaled consensus tracking problem for multi-agent systems (MASs) with unknown dynamics is studied in this paper under a directed graph. Each agent's dynamic is described by a high-order differential equation, which includes time delays and uncertainty. A novel fully distributed adaptive control method is proposed to ensure that all following agents track the leader with prescribed scaled ratios asymptotically, and the tracking errors are uniform ultimate boundedness. Meantime the proposed approach guarantees the bounded adaptive estimates of the unknown parameters and dynamics, and can lighten the burden on computation because it only needs to update a small number of adaptive parameters. The control gain of each agent is heterogeneous and time-varying. It changes and updates by the designed distributed adaptive law. The presented protocol doesn't need to know global nonzero eigenvalues of communication graph adjacency matrix, and is independent of any global information. Finally, the effectiveness of the theoretical results is verified by a numerical example.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {491–497},
numpages = {7}
}

@inbook{10.1145/3728725.3728832,
author = {Yang, Wangfeng and Li, Zhongyang},
title = {Research on the Development and Application of Digital Twin Technology in the Field of Aircraft Maintenance and Support},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728832},
abstract = {With the development of science and technology, the modernization of equipment is accelerated, the application of digital twin technology in the field of aircraft maintenance and support has been moving from concept to reality and showing its unique and broad prospect. The combination of digital twin technology and intelligent maintenance technology can realize equipment failure prediction, rapid positioning and maintenance in order to increase the production efficiency and reduce the production cost. This paper introduces the research status of digital twin technology and its characteristics, discusses the obvious advantages in the field of aircraft maintenance and support and the possible development trend in the future. For aircraft, the application of digital twin in maintenance support is realized, the digital twin framework is constructed, and the simulation model of aircraft is established. The efficiency and reliability of maintenance support for aircraft are improved. This case has been applied in outfield aircraft support and can be further extended to the other equipment.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {680–684},
numpages = {5}
}

@inbook{10.1145/3728725.3728799,
author = {Wen, Chengjiang and Yue, Yang and Wang, Zhixiang},
title = {The Application of Membership Inference in Privacy Auditing of Large Language Models Based on Fine-Tuning Method},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728799},
abstract = {Thanks to the large datasets collected from the internet, large language models (LLMs) have made significant progress across various fields, with widespread applications in digital employees, intelligent customer service, and smart decision-making. However, during the training process, models may memorize sensitive private data, posing risks to data security and privacy protection. Member Inference Attacks (MIA) aim to determine whether a specific record was used in the training of the target model. This approach is crucial for detecting privacy risks in models and has seen substantial progress in both traditional and fine-tuned models. However, this remains an unresolved issue for large language models (LLMs). Recent studies indicate that under proper experimental setups, the AUC-ROC of existing methods approaches 0.5 (random guessing). Subsequently, some research has shifted MIA to dataset inference, achieving promising results in this area. Our study shows that when fine-tuning a target model with aggregated samples, it is possible to distinguish whether these samples were used in training the target model by observing the initial loss returned by the model. Based on this observation, we propose a fine-tuning-based member inference method for large language models. To validate the effectiveness of our method, we conducted extensive experiments on three mainstream models and seven datasets. The results indicate that member inference attacks with aggregated samples can achieve acceptable outcomes.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {473–479},
numpages = {7}
}

@inbook{10.1145/3728725.3728811,
author = {Zhu, Minghua},
title = {Solving Class Imbalance in Medical Image Classification Based on Federated Learning},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728811},
abstract = {Federated Learning (FL) has excellent privacy-preserving characteristics and is widely used in the medical field. However, medical data often suffer from class imbalance, which can affect the performance of models. In this paper, we first introduce a novel privacy-preserving federated learning scheme that addresses the class imbalance problem through both feature learning and classifier learning. In the feature learning stage, two feature extraction networks are used in parallel and optimized. Finally, in the classifier learning stage, multiple loss functions are designed to ensure balanced learning across all classes. Experimental results on a publicly available skin dataset with simulated multi-source medical image data demonstrate outstanding performance in handling class imbalance. The model achieves balanced classification accuracy, overall accuracy, and F1 score of 87.02%, 88.27%, and 87.33%, respectively. Compared to other models, the balanced accuracy, overall accuracy, and F1 score are improved by 8.71%, 3.61%, and 8.23%, respectively.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {546–550},
numpages = {5}
}

@inproceedings{10.1145/3631700.3664873,
author = {Loepp, Benedikt and Ziegler, J\"{u}rgen},
title = {Exploring the Potential of Generative AI for Augmenting Choice-Based Preference Elicitation in Recommender Systems},
year = {2024},
isbn = {9798400704666},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3631700.3664873},
doi = {10.1145/3631700.3664873},
abstract = {The recent boost in generative artificial intelligence has also reached the field of recommender systems. However, as is often the case, much of the work focuses on the algorithms, overlooking the crucial aspect of improving the systems from a user perspective. In this initial research, we explore the potential of large language models to achieve improvements in preference elicitation. The interactive choice-based method we are augmenting has previously demonstrated significant improvements in a number of aspects related to the user experience. Through an exploratory user study, we show that the item set comparisons presented by this method can be successfully accompanied by independently generated textual summaries, thereby improving the user experience even further.},
booktitle = {Adjunct Proceedings of the 32nd ACM Conference on User Modeling, Adaptation and Personalization},
pages = {114–119},
numpages = {6},
keywords = {Large language models, Preference elicitation, Recommender systems, User experience.},
location = {Cagliari, Italy},
series = {UMAP Adjunct '24}
}

@inbook{10.1145/3728725.3728826,
author = {Li, Shulei and Meng, Bo and Wang, Dejun},
title = {Event Extraction Methods for Government Hotline Domains},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728826},
abstract = {Event extraction for government hotlines is crucial for improving scheduling efficiency and optimizing resource allocation, but varying text lengths and diverse event types increase the extraction difficulty. To this end, we propose a method that fuses the spanning mechanism with machine reading comprehension to first extract trigger words and predict event types, and then extract event arguments using customized problem templates. Experimental results show that the method outperforms traditional methods in terms of precision, recall, and F1 value, and can effectively identify public service demands and emergency events, providing support for government information integration and decision optimization.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {643–647},
numpages = {5}
}

@inbook{10.1145/3728725.3728821,
author = {Cai, Litao and Liu, Mingmei and Feng, Youbing and Xiong, Feng},
title = {Research on fall detection algorithm based on improved YOLOv8},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728821},
abstract = {Aiming at the traditional fall detection algorithms that suffer from low detection accuracy and poor real-time performance in dealing with small targets and multi-scale features in complex scenes, this study proposes a fall detection algorithm based on an improved YOLOv8 model. First, the SCConv module is utilized to combine with the C2f module in the YOLOv8 backbone network to construct the C2fSC module, which enhances the feature extraction capability of the model while reducing the computational complexity. Second, the SPPF module is integrated with the BAM attention mechanism to weaken the influence of background interference. Finally, WIoU is used to replace the original loss function to optimize the positioning accuracy of the bounding box and enhance the overall performance of the model. The experimental results show that compared with the original YOLOv8 model, the improved model improves 4.9%, 4.2%, and 4.4% in P, R, and mAP metrics, respectively, and the number of parameters and FLOPs are only 2.8M and 7.7G, which proves that the improved model has excellent robustness and effectiveness.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {610–615},
numpages = {6}
}

@inbook{10.1145/3728725.3728804,
author = {Wang, Zheng and Jia, Xuezeng and Liu, Liya},
title = {Analysis of occupant escape from indoor building fire based on optimized cellular automaton model},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728804},
abstract = {Indoor decoration materials are increasingly used in buildings. Most of them are combustible or even flammable, which makes them potential fire sources and increases the fire load of buildings. Based on the cellular automaton theory and the multi-lattice theoretical calculation model, this paper simulates the temperature and heat release rate in indoor fires. The results show that the model can better simulate the accuracy of personnel evacuation in indoor fires.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {505–508},
numpages = {4}
}

@inbook{10.1145/3728725.3728782,
author = {Wang, Jingsi and Li, Xiaobin and Zhu, Pengyu and Chen, Zhuoyi},
title = {Identification of personnel identity in unmanned aerial vehicle video gait recognition based on 3Dresidual network},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728782},
abstract = {An improved adaptive 3D ResNet-18 residual network gait recognition method is proposed for the problem of recognizing people's identity in videos captured by UAVs. Using a combination of gait feature extraction and deep learning models, human silhouettes are separated from complex backgrounds for modeling, and their gait features are extracted for identification.3D ResNet-18 residual network learns spatio-temporal features to extract temporal and spatial features in the sequence of video frames and introduces a cross-entropy loss function to optimize the model and improve the prediction accuracy. When dealing with the challenges of motion blurring in the video captured by drones and shadows and noise interference in background modeling, the improved MOG2 background separation algorithm detects pedestrian gait and models it, and adaptive variance thresholding improves the background detection capability and improves the spatio-temporal feature extraction and optimization, which significantly improves the robustness of the model. The experiments use the Walking category in the KTH public dataset, and the results show that the model exhibits good recognition results in different backgrounds. This approach effectively improves the performance of UAVs in applications such as security surveillance.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {354–358},
numpages = {5}
}

@inbook{10.1145/3728725.3728737,
author = {Lei, Zhongxia and Zhang, Caixia and Gui, Junzhen},
title = {Applications and Comprehensive Evaluation of Commodity Reconstruction based on NeRF},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728737},
abstract = {NeRF, as an implicit 3D scene generation algorithm, has attracted widespread attention in the field of 3D reconstruction. However, its application in the field of small commodity 3D modeling has not been fully evaluated. To offer specific technical guidance for the application of Nerf in the e - commerce field, we perform detailed experiments and comparisons from multiple aspects such as the complexity of the background, lighting conditions, the number of images, and the surface texture of the experiment.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {72–79},
numpages = {8}
}

@inbook{10.1145/3728725.3728778,
author = {Sun, Minghai and Cheng, Yufan},
title = {Research and application of K-means clustering based on the color characteristics of woodblock prints of Zhuxianzhen},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728778},
abstract = {Objective This study aims to quantitatively analyze the color composition of woodblock prints of Zhuxianzhen, explore its color combination patterns, and provide a theoretical basis for the color design of cultural and creative products, so as to more accurately inherit and present the visual characteristics of this traditional Chinese folk art. Methods In this study, K-means clustering algorithm is used to extract color features from selected sample images of woodblock prints of Zhuxianzhen, and a color association network model based on the priority of the main color is constructed by calculating the proportion of color distribution and its combination frequency. Based on the results of the analysis of the color matching law of this model, the practice of innovative visual design and cultural and creative product development of Zhuxianzhen woodblock prints is carried out. Conclusion The results of the research present the color matching and color matching law of Zhuxianzhen woodblock prints in the form of icons, which can reflect the color characteristics of Zhuxianzhen woodblock prints in a more objective way, provide the designers with scientific and rich color matching solutions, improve the efficiency of designers' work, and at the same time, promote the re-design application of the traditional Chinese folk art.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {332–336},
numpages = {5}
}

@inbook{10.1145/3728725.3728816,
author = {Zhu, Qifei and Tang, Jiangang and Cao, Haoshun},
title = {An Improved LSTM Crime Prediction Model Based on Environmental Factors},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728816},
abstract = {Crime prediction serves as a fundamental basis for preventing crime, maintaining social stability, and ensuring public safety. Current crime prediction models commonly suffer from issues such as low accuracy, unstable predictions, or weak generalization capabilities. Therefore, we constructed an improved LSTM crime prediction model based on environmental factors, integrated the data of various environmental factors such as economy, culture and climate, improved the LSTM network structure and optimization method, and enhanced the prediction ability of crime in the region. Empirical evidence shows that the MAPE predicted by the model is 0.0491 and R2 is 0.9229. Compared with the traditional model, the accuracy, stability and generalization ability of the prediction are significantly improved, which can provide a basis for the formulation of crime prevention and control strategies, improve the level of social security management, and promote social stability.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {577–584},
numpages = {8}
}

@inbook{10.1145/3728725.3728781,
author = {Li, Chan and Guo, Wenjing and Qin, Jikai},
title = {Backward automatic screening feature selection algorithm based on multi-evaluation criterion fusion},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728781},
abstract = {In view of the tedious problem that the feature selection algorithm based on multi-evaluation criterion fusion needs to manually set the feature threshold repeatedly to obtain the optimal feature subset, a backward automatic selection feature selection algorithm based on multi-evaluation criteria fusion is proposed in this paper. In this algorithm, the feature evaluation value is calculated using multi-evaluation criteria fusion algorithm, then the discrimination performance of Support Vector Data Description (SVDD) is used as the screening criterion, and finally the optimal feature subset is obtained by backward automatic screening algorithm. The experimental results show that the feature selection algorithm has the advantages of automatic screening, noise robust, clutter robust, low feature dimension, high discrimination rate and low deterioration of discriminative performance for small sample problems.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {347–353},
numpages = {7}
}

@inbook{10.1145/3728725.3728831,
author = {Li, Chengxuan and Yang, Wei and Sun, Chang},
title = {UVD-ATrack: UAV-based Vehicle Detection and Adaptive Tracking},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728831},
abstract = {In recent years, the increasing number of vehicles has led to heightened traffic congestion. Traditional surveillance is limited to monitoring fixed locations. However, unmanned aerial vehicle (UAV) photography provides advantages such as convenience and a wide perspective, gradually replacing these conventional techniques for traffic video collection. Nonetheless, challenges including complex scenes, varying scales and frequent occlusions result in issues such as false positives, missed targets, and identity switches (IDSW). To tackle these, we developed the UVD detection algorithm based on YOLOv8-n and the AByteTrack tracking algorithm derived from ByteTrack. The integration of these two components forms the UVD-ATrack algorithm. In the backbone of the detection section, we introduced the RFAConv and designed RF_C2f to enhance feature extraction capabilities. For the neck component, the adaptive weight fusion module (AWFM) was developed to effectively combine multi-scale local information. During the tracking phase, we optimized the motion model. Additionally, camera motion compensation (CMC) was implemented to adaptively mitigate the effects of sharp perspective changes. Test results utilizing the Visdrone dataset indicate that mAP@0.5 and mAP@0.5:0.95 improved by 4.4% and 3.6% respectively in the detection section. In the tracking test, IDF1, HOTA, MOTA and MOTP also exhibited significant enhancement while IDSW decreased by 77%. Compared with state-of-the-art methods, our proposed algorithm demonstrates exceptional performance.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {673–679},
numpages = {7}
}

@inbook{10.1145/3728725.3728823,
author = {Wu, Chunyang},
title = {Pneumonia Detection and Classification Using Lightweight ShuffleNetV2 Based on Transfer Learning},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728823},
abstract = {Pneumonia, a severe respiratory infection, is commonly caused by either viral or bacterial pathogens. Timely identification of pneumonia is essential for effective patient care and treatment. Chest X-ray imaging is widely utilized for both diagnosing and detecting pneumonia, though interpreting these images presents certain difficulties. This research leverages transfer learning to evaluate and compare the performance of various convolutional neural networks (CNNs) in the context of chest X-ray image classification and detection. The results of the experiment demonstrate that the lightweight ShuffleNetV2 model excels in minimizing computational burden and memory consumption, while preserving high accuracy and effectiveness in classification tasks.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {621–625},
numpages = {5}
}

@inbook{10.1145/3728725.3728759,
author = {Xiong, Yuzhu and Du, Hongwei and Zhang, Daikun},
title = {A Guided-learning Network for Brain Tumor Segmentation with Missing Modalities},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728759},
abstract = {Accurate segmentation of brain tumors is essential for clinical assessment and treatment planning, as it leverages multiple MRI modalities to provide complementary diagnostic information. However, missing modalities are frequently en- countered in clinical practice due to factors such as image degradation, artifacts, protocol inconsistencies, or patient con- traindications to contrast agents. While certain unified models are designed to accommodate various missing modality scenarios, their performance declines markedly when multiple modalities are absent, particularly in cases where only a single modality is available. To tackle this challenge, we introduce a Guided- learning Network that leverages dynamic FFT filters. This network trains multiple specialized models designed for different missing modality scenarios, achieving outstanding performance. The architecture comprises two distinct learning paths: one dedicated to capturing multimodal information and another focused on generating modality-specific representations for the missing modality. A guided-learning strategy connects these two paths. Initially, the multimodal path is trained and its parameters are locked, enabling the missing modality path to learn under its guidance. The training process incorporates several key com- ponents: adversarial learning to align high-level features across paths, frequency-domain adaptive filtering using dynamic FFT filters to extract global features, and mutual information transfer to ensure critical information is retained even when modality data is missing. Experiments conducted on the BraTS2018 dataset demonstrate that our model outperforms comparable state-of- the-art methods in all missing modality cases.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {219–225},
numpages = {7}
}

@inbook{10.1145/3728725.3728739,
author = {Yi, Jingyuan and Xu, Zeqiu and Huang, Tianyi and Yu, Peiyang},
title = {Challenges and Innovations in LLM-Powered Fake News Detection: A Synthesis of Approaches and Future Directions},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728739},
abstract = {The pervasiveness of the dissemination of fake news through social media platforms poses critical risks to the trust of the general public, societal stability, and democratic institutions. This challenge calls for novel methodologies in detection, which can keep pace with the dynamic and multi-modal nature of misinformation. Recent works include powering the detection using large language model advances in multimodal frameworks, methodologies using graphs, and adversarial training in the literature of fake news. Based on the different approaches which can bring success, some key highlights will be underlined: enhanced LLM-improves accuracy through more advanced semantics and cross-modality fusion for robust detections. The review further identifies critical gaps in adaptability to dynamic social media trends, real-time, and cross-platform detection capabilities. Future directions underline the development of style-agnostic models, cross-lingual detection frameworks, and robust policies with a view to mitigating LLM-driven misinformation. This synthesis thus lays a concrete foundation for those researchers and practitioners committed to reinforcing fake news detection systems with complications that keep on growing in the digital landscape.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {87–93},
numpages = {7}
}

@inbook{10.1145/3728725.3728815,
author = {Zhu, Yiqi},
title = {Multi-label classification of medical images based on feature fusion},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728815},
abstract = {This article considers the role of multi-label medical image classification and the related revolution in clinical diagnosis. Here, the use of deep learning technologies brings up the problem of single-label classification methods that may not be able to deal with the multi-label one as they do not cover the complex, sometimes coexisting, multiple-pathological features in medical images. To address these challenges, we propose the CTTransNet speculative model, a combination of Convolutional Neural Networks (CNNs) and Vision Transformers (ViTs), which aims to better integrate the local features and the global context. Similarly, a Transformer decoder is used to model inter-label dependencies, and a less-weighted fusing loss mechanism is utilized to deal with the data imbalance issue. The AUC of 81.68% on the ChestXray14 dataset is achieved by the proposed method, which shows its higher accuracy and superiority to the robustness compared to the traditional methods in multi-label medical image classification. This breakthrough thus brings about the complete information needed for a clinical diagnosis and makes it easier for personalized precision medicine to develop.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {570–576},
numpages = {7}
}

@inproceedings{10.1145/3613905.3651057,
author = {Lyu, Yao and Zhang, He and Niu, Shuo and Cai, Jie},
title = {A Preliminary Exploration of YouTubers' Use of Generative-AI in Content Creation},
year = {2024},
isbn = {9798400703317},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3613905.3651057},
doi = {10.1145/3613905.3651057},
abstract = {Content creators increasingly utilize generative artificial intelligence (Gen-AI) on platforms such as YouTube, TikTok, Instagram, and various blogging sites to produce imaginative images, AI-generated videos, and articles using Large Language Models (LLMs). Despite its growing popularity, there remains an underexplored area concerning the specific domains where AI-generated content is being applied, and the methodologies content creators employ with Gen-AI tools during the creation process. This study initially explores this emerging area through a qualitative analysis of 68 YouTube videos demonstrating Gen-AI usage. Our research focuses on identifying the content domains, the variety of tools used, the activities performed, and the nature of the final products generated by Gen-AI in the context of user-generated content.},
booktitle = {Extended Abstracts of the CHI Conference on Human Factors in Computing Systems},
articleno = {20},
numpages = {7},
keywords = {Affiliated Marketing, Artificial Intelligence, Content Creation, Content Creator, Generative AI, Professional Development, User-generated Content, YouTube},
location = {Honolulu, HI, USA},
series = {CHI EA '24}
}

@inproceedings{10.1145/3728725.3728818,
author = {Zhu, Qifei and Tang, Jiangang and Cao, Haoshun},
title = {An Internet Public Opinion Monitoring Model Based on Stacked Ensemble Learning},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728818},
doi = {10.1145/3728725.3728818},
abstract = {With the rapid development of social media, internet public opinion has a profound impact on social stability and economic development. At present, common monitoring models generally have problems such as insufficient accuracy or weak generalization ability. Therefore, This paper proposes an internet public opinion monitoring model based on stacked ensemble learning, which aims to realize the real-time monitoring. The model selects combines random forest (RF), logistic regression (LR), support vector machine (SVM) group as the base learner. And integrates the prediction results of the base-learners through the meta learners (GBM) to monitor public opinion in real time. The experimental results show that the empirical model achieves 0.940 accuracy and an F1 score of 0.896, which significantly outperforms other ensemble methods. It can effectively monitor the scale and trend of public opinion of internet comments, and provide strong support for decision makers.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {591–597},
numpages = {7},
keywords = {Empirical Research, Internet Public Opinion Monitoring, Stacked Ensemble Learning},
location = {
},
series = {GAIIS '25}
}

@inbook{10.1145/3728725.3728754,
author = {Sun, Qinhui and Gao, Bin},
title = {Emotion-enhanced Cross-modal Contrastive Learning for Fake News Detection},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728754},
abstract = {This paper proposes a fake news detection model based on emotion-enhanced cross-modal contrastive learning (ECC-FND), which aims to fully utilize the information from images, text, and user comments in social media news to improve the accuracy and efficiency of fake news detection. ECC-FND combines cross-modal contrastive learning and sentiment analysis methods to effectively explore the semantic associations between multimodal information and dynamically weights features from different modalities using an attention mechanism. Experimental results demonstrate that ECC-FND significantly outperforms existing state-of-the-art methods on the Weibo dataset, which validates its effectiveness and generalization ability in fake news detection tasks. This study provides new insights for multimodal fake news detection and offers valuable references for future research.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {184–189},
numpages = {6}
}

@inbook{10.1145/3728725.3728796,
author = {Sun, Fuyue},
title = {The Fan Anomaly Detection Method Based on an Improved CBAM Attention Mechanism with MobileNetV3},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728796},
abstract = {In factories, fans is an important part in production. However, how to ensure the safe and stable operation of it is always a difficult problem. In order to solve this problem, this study presents a fan anomaly detection method based on an improved CBAM attention mechanism using the MobileNetV3-Small model. In this model, the conventional SE module is replaced with an optimized CBAM, which uses dilated convolutions to enhance the model's sensitivity to anomalous sounds. At the same time, the model incorporates CutMix and Focal Loss as the loss function, further improving the model's robustness to imbalanced data. Additionally, to strengthen the nonlinear representation capacity of the architecture, we implement CELU activation functions as substitutes for conventional ReLU units within residual modules. According to experimental results, we can find that the proposed model achieves a classification accuracy of 97% in a -6dB noise environment, which is an improvement of 1.07 percentage points over the original MobileNetV3 model, demonstrating the effectiveness of this approach in fan anomaly detection.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {453–458},
numpages = {6}
}

@inbook{10.1145/3728725.3728780,
author = {Lin, Zhenyang},
title = {Metal Level Measurement and Compensation Based on the MiDas Model},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728780},
abstract = {Accurate measurement of liquid metal levels is critical to the safety and efficiency of industrial process monitoring and automation. Traditional contact-based measurement methods face significant limitations in high temperature and extreme environments, such as safety risks, inaccurate measurements due to interference with the environment, and short equipment life. To address these challenges, this study proposes an innovative non-contact measurement method using a visual depth estimation technique based on deep learning. The MiDaS model captures an image of a liquid metal surface by a monocular camera in an industrial environment. A deep neural network then estimates their depth by processing these images. By calculating the vertical distance between the liquid metal level point and a predefined reference point, real-time and accurate monitoring of the liquid level height is realized. This study not only introduces a new non-contact solution for measuring the height of liquid metal surfaces, but also enhances the potential applications of deep learning in industrial vision measurement and depth estimation.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {342–346},
numpages = {5}
}

@inbook{10.1145/3728725.3728803,
author = {Yao, Xiongfei},
title = {Demand Forecasting for Cross-border Retail Commodities in the New Retail Era: A Hybrid Model Integrating Temporal Granularity and Categorical Dynamics},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728803},
abstract = {Through the lens of the Temporal-Synergic Cross-Correlation (TSCC) framework, this study fills important gaps in cross-border demand forecasting. Overall, analyzing 9,754 transactions (2022–2024) our approach captures multi-scale temporal decomposition synergized with SHAP-based feature engineering and outperforms the LSTM-RF hybrids by 22% precision and discovers 14.7% YoY revenue growth. Notably, the innovations tackled temporal-categorical disconnects and 92% holiday prediction precision — 18.4% on global metrics. The framework bridges theoretical models and operational logistics, harvesting trials and errors to overcome cross-border commerce lead-time preserves.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {498–504},
numpages = {7}
}

@inbook{10.1145/3728725.3728805,
author = {Chen, Hongfu and Jin, Minghui and Gao, Feng and Zhai, Yujia},
title = {Research and Implementation of Access Control Strategy Based on Apriori and Collaborative Filtering Techniques},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728805},
abstract = {This paper presents an automated method for generating access control policies by combining the Apriori algorithm and collaborative filtering technology. This approach solves the complex access control challenges arising from data sharing and large number of users in power grid regulation systems. First, the Aprior algorithm is used to mine strong association rules from the user authority relationship to reveal the potential relationship between resources. Then, the user rights relationship is enhanced, resource similarity is calculated, and collaborative filtering is used to generate recommended values for unauthorized resources. Finally, the access control policy is classified and authorized according to the recommended value, and the automatic generation of the policy is realized. The experimental results show that this method can not only generate access control policies automatically, but also assign new permissions effectively and ensure the privacy of resource data.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {509–514},
numpages = {6}
}

@inbook{10.1145/3728725.3728743,
author = {Dong, Benchang and Chen, Zhili},
title = {PPQformer: Privacy-Preserving Quantized Transformer for Efficient and Secure Inference.},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728743},
abstract = {In the context of the popularity of Transformer model services, the issue of privacy protection has gradually gained attention. However, the primary issues with existing private inference solutions lie in memory constraints and suboptimal computational performance. In this work, we introduce PPQformer, a framework that leverages Replicated Secret Sharing (RSS) and quantization techniques to enable efficient and secure inference of Transformer models in a Multi-Party Computation (MPC) setting. By integrating quantization into the MPC protocols, PPQformer significantly reduces memory footprint and enhances computational performance while preserving data and model privacy. Experimental results demonstrate the effectiveness of our approach, achieving competitive accuracies with reduced communication cost and inference time compared to existing methods.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {115–119},
numpages = {5}
}

@inbook{10.1145/3728725.3728783,
author = {Wang, Qiaoxin and Zheng, Shouguo and Qiu, Mengqing and Hu, Dan},
title = {Detection of pesticide residues by sensor arrays fused from SERS spectra of various substrates combined with deep learning},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728783},
abstract = {In this study, a rapid, accurate and intelligent method for detecting pesticide residues was developed by sensor arrays fused from surface-enhanced Raman spectroscopy (SERS) spectra of various substrates combined with deep learning. First, spectral data of pesticide residues were collected using conventional substrates with various morphologies. Then, the sensor array is acquired using different fusion methods on the spectral data, specifically concatenation and parallel methods. Finally, the intelligent and precise detection of chlormequat chloride and paraquat is implemented in a regression model constructed through the sensor array combined with machine learning and deep learning. In this experiment, the lowest detection limits for chlormequat chloride and paraquat detected are 0.03 mg/L and 0.003 mg/L, respectively. The regression model which was constructed using the sensor arrays obtained from the concatenation fusion method combined with the MobileNet obtained the best prediction results, with the coefficients of determination of prediction of 0.9865 and 0.9846 for chlormequat chloride and paraquat, and root-mean-square errors of prediction of 0.2254 and 0.0174, respectively. Overall, the method we proposed achieves rapid, intelligent and accurate detection of chlormequat chloride and paraquat, with the advantages of low cost and simple operation, which has great application potential in trace detection of pesticide residues.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {359–366},
numpages = {8}
}

@inproceedings{10.1145/3728725.3728819,
author = {Li, Mingyang and Li, Pengpeng and Wang, Rui and Mou, Weihua},
title = {Residual Neural Network with Attention Mechanism for Direction-of-Arrival Estimation in Low SNR},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728819},
doi = {10.1145/3728725.3728819},
abstract = {In this paper, we propose an improved residual neural network (ResNet) based on an attention mechanism for estimating the direction of arrival (DOA) of linear arrays. The real, imaginary, and phase components of the covariance matrix are used as inputs to an auto-encoder (AE), whose output is then introduced into a cascaded ResNet for DOA estimation. To enhance estimation accuracy, an attention mechanism is integrated into the ResNet, effectively assigning greater weight to the most relevant features. Experimental results demonstrate the proposed method's superior DOA estimation performance compared to existing methods, especially in low SNR.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {598–604},
numpages = {7},
keywords = {Direction of arrival estimation, attention mechanism, auto-encoder, residual neural network},
location = {
},
series = {GAIIS '25}
}

@inbook{10.1145/3728725.3728768,
author = {Yu, Ze and Miao, Quanqiang},
title = {Cybersecurity Survivability Testing Technology Based on ATT&amp;CK and D3FEND},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728768},
abstract = {In the context of the continuous evolution of current network threats, organizations urgently need advanced cybersecurity defense strategies to deal with complex and changeable network attacks. Survivability testing is usually implemented by simulating real hacker attacks, but this kind of testing and evaluation method depends on the professional level of testers to a large extent. There are problems such as incomplete testing and low testing efficiency. This paper comprehensively utilizes the ATT&amp;CK (Adversarial Tactics, Technologies, and Common Knowledge) and D3FEND (Defensive Tactics, Technologies, and Common Knowledge) frameworks of MITRE, aiming to propose a new cybersecurity survivability testing technology. Through in-depth analysis of the attack tactics, techniques and procedures (TTPs) in the ATT&amp;CK framework, combined with the defense strategies and measures in the D3FEND framework, this paper designs a cybersecurity survivability evaluation program. This scheme not only evaluates the protection, detection, response and recovery capabilities of the system against network attacks, but also provides a scientific and perfect defense measures for the system. The case study further verifies the effectiveness of the proposed scheme, and shows how to conduct survivability testing against attacks on the Linux operating system. This study provides a new perspective and practical guidance for the development of cybersecurity defense technology.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {272–277},
numpages = {6}
}

@inbook{10.1145/3728725.3728793,
author = {Chen, Jian and Wang, Lei and Jiang, Jun and Liu, Ruitong and Ji, Heng and Zhao, Yuntao},
title = {A Knowledge Reasoning Modeling for security vulnerability databases based on Gaussian-SVM hybrid clustering},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728793},
abstract = {At present, network security becomes more and more important, but the security vulnerability data in various databases are relatively dispersed and it is difficult to establish the connection between them, network security knowledge graph can associate the security vulnerability data of different databases together, and it can be updated in real time with the latest security vulnerabilities. This paper establishes network security knowledge graph based on CAPEC, CWE and other security libraries. Aiming at the problem that TransR model cannot utilize entity semantic information, TransR-Roberta model is proposed, and splicing layer, attention layer and convolutional layer are added on the basis of TransR model. And for the problem that the quality of negative samples generated by Bernoulli negative sampling is too low, a clustering-based negative sampling optimization method is proposed. Firstly, the clustering task is accomplished with the help of Gaussian hybrid clustering algorithm on the entities in the security vulnerability knowledge graph to label the entities with clusters. In the next step, the clustered vulnerability entities are reclassified using support vector machine. Finally, a classifier combined with Facolloss function is added to the loss function of the TransR model, which is used to improve the ability of the knowledge inference model to distinguish between positive and negative samples. The experimental results show that the improved model improves the hit@N, MR and MMR metrics for the knowledge reasoning task.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {435–439},
numpages = {5}
}

@inbook{10.1145/3728725.3728760,
author = {Dong, Bingchen and Wu, Gengshen},
title = {A Swin Transformer-Based Cold Diffusion Model for Accelerated MRI Reconstruction},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728760},
abstract = {Clinical diagnosis has become easier because of magnetic resonance imaging (MRI) since detailed images of soft tissues are possible. While having its own advantages MRI has multiple limitations. MRI machines are generally expensive, and extended scan time can (over)burden clinical healthcare systems at the expense of medical work efficiency. The rise of deep learning technologies has spurred the development of methods to accelerate MRI reconstruction. In this regard, we introduce a novel cold-diffusion-based approach to accelerate MRI reconstruction. This method leverages the Swin Transformer architecture for effective feature extraction and incorporates a high-frequency self-attention mechanism to improve the recovery of high-frequency components, thus enhancing image quality. Unlike traditional diffusion models that rely on Gaussian noise, our cold-diffusion model simulates data loss through specific degradation processes like Cartesian undersampling. Experimental results demonstrate that the Swin-CDMR approach outperforms existing techniques in reconstruction quality, achieving superior structural similarity index (SSIM) and peak signal-to-noise ratio (PSNR) scores while maintaining robustness across varying acceleration factors (AF).},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {226–230},
numpages = {5}
}

@inbook{10.1145/3728725.3728808,
author = {Chen, Siqi and Zhang, Wenzhen and Chen, Xinye and Ji, Yuchao and Liu, Xinyu and Yu, Fan and Xu, Huazhen},
title = {Plant disease detection technology based on hybrid convolutional attention mechanism},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728808},
abstract = {As the scale of agricultural production continues to expand, plant diseases have become a major threat to global agricultural productivity. Although traditional disease detection methods are widely used, they generally suffer from low accuracy and complex operations, making them difficult to meet the needs of modern agriculture. In response to these problems, this article proposes a new deep learning model-LabInt. LabInt uses dual-branch independent convolution in Lab color space, which effectively reduces the impact of illumination changes on feature extraction. Through spatial reconstruction and multi-scale pooling technology, the model can deeply mine shallow disease features and use deep convolution instead of ordinary convolution to reduce computational costs and improve the model's fine-grained identification capabilities. In addition, in order to adapt to complex agricultural environments, LabInt introduces a multi-head self-attention (MHA) mechanism to dynamically enhance attention to disease characteristics and significantly reduce the interference of complex backgrounds. Experimental results show that LabInt exhibits excellent disease recognition performance in actual agricultural production environments, with key indicators such as precision and recall reaching high levels. Ablation studies show that each module makes an important contribution to model performance. Therefore, LabInt is not only innovative in theory, but also shows strong application potential in practice, and is expected to become an important tool to improve the efficiency of agricultural disease prevention and control.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {526–531},
numpages = {6}
}

@inbook{10.1145/3728725.3728810,
author = {Li, Wei and Zhang, Kangwei and Li, Penglin and Yan, Keyang},
title = {DAT-YOLO: Small Object Detection Model from the Perspective of Drones},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728810},
abstract = {A DAT-YOLO model incorporating deformable attention is proposed to address the challenges of dense target distribution, small pixel size, and uneven sample size in object detection from the perspective of unmanned aerial vehicles. This model is based on YOLOv8s and optimizes the Backbone and Neck structures, allowing the model network to be shallower in depth while being more focused on small targets; Using RepNCSPELAN4 structure instead of C2f structure to better adapt the model to different computing and hardware requirements; The deformable attention mechanism DAT is integrated into the spatial pyramid pooling layer, allowing the network to capture more effective features while expanding the receptive field; The use of inner-ShapeIoU loss function improves the algorithm's localization and classification performance for small targets in complex environments. The experimental results of DAT-YOLO on the VisDrone dataset show that with a 57.5% reduction in parameter count, the mAP@50 and the mAP@50:95 has also increased by 6.7% and 5.0% respectively, making it suitable for drone target detection tasks.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {538–545},
numpages = {8}
}

@inbook{10.1145/3728725.3728750,
author = {Yang, Tonghui and Xu, Yamei},
title = {Super-resolution Reconstruction of Remote Sensing Images with Improved SwinIR Transformer},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728750},
abstract = {Aiming at the problem of large differences in feature information between different targets in remote sensing images and the complexity of the scene, an improved SwinIR Transformer remote sensing image super-resolution reconstruction algorithm is proposed, combining residual hybrid attention and spatial-gate feed-forward network. First, a new residual hybrid attention module (RHAB) is constructed, which is serially combined with multiple self-attention (MSA) to enhance feature extraction by mixing channel attention (CA), spatial attention (SA), and MSA, and to improve the model's ability in modeling locally and globally relevant information. Second, a spatial-gate feed-forward network (SGFN) is used instead of the multilayer perceptron (MLP) in the original network to effectively reduce redundant channel information, overcome the limitations of the MLP in dealing with local spatial features, and enhance the feature expression efficiency. The experiments use NWPU-RESISC45 and UCMerced_LandUse as the training set, RSOD-Dataset, RSSCN7 Data Set, and WHU-RS19 Data Set as the test set, and the model performance is evaluated by PSNR and SSIM metrics. The experimental results confirm that the model in this paper shows excellent performance in both objective evaluation and subjective visualization.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {157–162},
numpages = {6}
}

@inbook{10.1145/3728725.3728756,
author = {Wu, Keyu and Fu, Jingyi and Zhang, Wenfang},
title = {Aspect-based Capsule Neural Networks for Sentiment Analysis of Sensitive Information},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728756},
abstract = {To address the need for accurate sentiment masking of sensitive words in sensitive information, traditional sentiment analysis methods can only assess the overall polarity of a sentence, while existing Aspect-Based Sentiment Analysis (ABSA) mostly focuses on the field of commodity reviews, and insufficiently handles phenomena such as complex contexts and irony. To this end, this paper proposes the CapABSA model, which takes sensitive words as aspects, combines capsule neural networks to capture multidimensional semantic features, and migrates document-based sentiment analysis (DBSA) to ABSA through transfer learning to improve classification performance. Experiments show that the model performs effectively in sentence datasets containing sensitive words, providing a more fine-grained solution for sensitive information filtering.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {196–201},
numpages = {6}
}

@inbook{10.1145/3728725.3728825,
author = {Zhou, Ziye and Guo, Yanbin and Tang, Xin and Li, Kehan and You, Heng},
title = {Dynamic Decision Correction Framework Integrating Path Optimization and Incomplete Information Handling},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728825},
abstract = {Despite significant advancements in multi-agent systems, solving the optimal path problem and addressing incomplete information remain challenging in multi-agent reinforcement learning (MARL). Traditional MARL algorithms often struggle with these two critical issues. To address these challenges, this paper proposes a novel framework called DDCF(Dynamic Decision Correction Framework) that integrates deep reinforcement learning (DRL) with Long Short-Term Memory (LSTM) networks to optimize the decision-making process for agents navigating complex environments. The first key innovation of this framework is the application of LSTM networks, which enable agents to learn optimal paths over time while considering dynamic, temporal dependencies within the environment. The second key innovation is the introduction of a dynamic decision-making approach, which effectively handles the problem of incomplete information by enabling agents to make decisions based on both observable and inferred states. This decision method ensures that agents can intelligently handle uncertain environments by incorporating both certain and fuzzy information. Experimental results demonstrate that the proposed framework significantly outperforms traditional methods in terms of path optimization and decision-making performance, providing a robust solution for agents in environments characterized by incomplete information and complex decision tasks.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {632–642},
numpages = {11}
}

@inbook{10.1145/3728725.3728800,
author = {Liu, Yang and Jin, Guanghu and Zhang, Jinji and Xue, Xiyue},
title = {Green quality perception of urban streets: a study combining semantic segmentation and multimodal noise enhancement techniques},
year = {2025},
isbn = {9798400713453},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728725.3728800},
abstract = {In recent years, the quantitative analysis of the green quality of urban streets has become a hot spot in the cross-research of computer vision and urban environment. In this paper, we propose a semantic segmentation method based on a dynamic multimodal noise addition technique combined with the SegFormer model to improve the robustness of street view image segmentation in complex environments. The study takes five major roads (West Beijing Road, North Taiping Road, Changjiang Road, Yihe Road, and Central Road) in Nanjing as objects, and based on the streetscape and remote sensing data of the vegetation growth season from 2017 to 2022, the Gaussian noise, motion blur, and weather effects are simulated by the multimodal noise addition technique, which effectively improves the adaptability of the model to the dynamic interference. The experiments showed that the Green View Index analysis revealed a general upward trend of street greening (average increase of 0.94%). Furthermore, the Kendall rank correlation analysis reveals the positive correlation between environmental factors and green visualization rate, and the influence law of environmental factors on green perception is verified. This study provides a high-precision computer vision analysis framework for urban greening planning and promotes the application of semantic segmentation models in complex urban scenes.},
booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
pages = {480–485},
numpages = {6}
}

@inproceedings{10.1145/3700297.3700350,
author = {Lin, Daping and Pu, Xianwei},
title = {Effects of Prompts and Time on the Automated Scoring of English Argumentative Essays by ChatGPT 4},
year = {2024},
isbn = {9798400707100},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3700297.3700350},
doi = {10.1145/3700297.3700350},
abstract = {As deep learning technology in computer science develops, generative artificial intelligence (GenAI) has shown great potential in automated essay scoring (AES). Prompts and time are important factors which may influence the performance of GenAI. The study selected 52 English argumentative essays, designed five different prompts, chooses two different time points, and then utilized ChatGPT 4 to explore the effects of prompts and time on AES by GenAI. Besides, possible reasons for large difference between human and GenAI score were discussed.For different prompts, results show that the one-shot prompt performs better in AES compared with the other four prompts. It provides background information and a scoring example to ChatGPT. The scores generated by it do not significantly differ from the human scores. They significantly and positively correlate with human scores (ρ = 0.424). The exact-plus-adjacent agreement (EPAA) rate for one-shot prompt scores reaches 69.23%. For scores generated at different points in time, results reveal that although there is still a significant difference between scores generated after one week and human scores, the ChatGPT-Human EPAA rate becomes higher and the absolute value of mean score difference is smaller.Based on the analysis of selected essays, the major reason for large GenAI-Human score difference is that ChatGPT evaluates essays from limited perspectives to give its score, while human raters can comprehensively assess the quality of an essay. What's more, ChatGPT cannot keep the same scoring criteria during the rating process.The study aims to help people understand how to interact with GenAI more efficiently and take advantage of GenAI to meet the practical needs.},
booktitle = {Proceedings of the 2024 International Symposium on Artificial Intelligence for Education},
pages = {302–312},
numpages = {11},
